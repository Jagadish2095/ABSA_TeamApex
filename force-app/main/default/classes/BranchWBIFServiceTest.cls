    @IsTest
    static void testCreatePolicyDetailsNoPreviousInsurer() {
        Test.setMock(HttpCalloutMock.class, new Sys_Utilities_Integration_Mock_ApiRest(StringConstants.POSITIVE_STRING, POSITIVE_RESPONSE_CREATE_POLICY));
        
        Test.startTest();   
        BranchAbsaLifeTestDataFactory testDataFactory = new BranchAbsaLifeTestDataFactory();
        String  opportunityProductId = testDataFactory.GetOpportunityProductId('WIMAL00103');
        String  oppId = testDataFactory.GetOpportunityId();
        String  acountId = testDataFactory.GetAccountId();
        
        Account account = new AccountsSelector().selectByOpportunityId(acountId, 1);
        List<Opportunity> opportunities = new OpportunitiesSelector().selectByOpportunityId(oppId);
        List<User> users = new UsersSelector().selectByOpportunityOwnerId(opportunities.get(0).OwnerId);
        
        WBIF_Product_Details.ExistingPolicy replacementPolicy = new WBIF_Product_Details.ExistingPolicy();
        replacementPolicy.ReplacementPolicy = false;
        replacementPolicy.PreviousInsurer = '';
        
        WBIF_IB_String_v1 serviceResp = BranchWBIFService.wbifPolicyCreate(opportunityProductId, users.get(0), account, opportunities.get(0), replacementPolicy, 'WIMAL00103');
        
        System.assertNotEquals(null,serviceResp);
        Test.stopTest();
    }   
    
