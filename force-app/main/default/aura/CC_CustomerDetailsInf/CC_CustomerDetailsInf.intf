  <aura:attribute type="String" name="communicationChannel" />
    <!--Employment Info Section-->
  <aura:attribute type="String" name="OccupationStatus" />
  <aura:attribute type="String" name="EmployerSector" />
  <aura:attribute type="String" name="OccupationCategory" />
  <aura:attribute type="String" name="OccupationLevel" />
  <aura:attribute type="String" name="numberOfYears" />
  <aura:attribute type="String" name="postMatricQualification" />
