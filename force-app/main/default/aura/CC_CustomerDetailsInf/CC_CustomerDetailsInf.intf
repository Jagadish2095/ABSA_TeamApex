  <aura:attribute name="idNumber" type="String" default="" />
  <aura:attribute name="genderValue" type="String" default="" />
  <aura:attribute name="nationality" type="String" default="" />
  <aura:attribute name="countryOfResidence" type="String" default="" />
  <aura:attribute name="initials" type="String" default="" />
  <aura:attribute name="lastName" type="String" default="" />
  <aura:attribute name="personBirthDate" type="String" default="" />
  <aura:attribute name="idType" type="String" default="" />
  <aura:attribute name="countryOfBirth" type="String" default="" />
