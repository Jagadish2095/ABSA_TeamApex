  <aura:attribute type="String" name="personEmail" />
  <aura:attribute type="String" name="personWorkPhone" />
  <aura:attribute type="String" name="personHomePhone" />
  <aura:attribute type="String" name="personMobilePhone" />
  <aura:attribute type="String" name="statementDeliveryMethod" default="email"/>
  <aura:attribute name="deliveryMethodOptions" type="List" default="[
  {'label': 'Customer would like to receive their statement on e-mail', 'value': 'Email'},
  {'label': 'Customer would like to post', 'value': 'Post'}
  ]" />
<!--Marital Status Information-->
  <aura:attribute type="String" name="maritalStatus" />
