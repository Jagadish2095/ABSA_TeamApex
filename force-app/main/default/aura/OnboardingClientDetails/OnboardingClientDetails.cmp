<!--
* Lightning Component for Onboarding Client Information (OnboardingClientDetails)
*
* @author  Manoj Danthala : Absa
* @version v1.0
* @since   2019-11-20
*
-->
<aura:component access="global" controller="OnboardingClientDetailsController"
                implements="lightning:isUrlAddressable,lightning:availableForFlowScreens,force:appHostable,flexipage:availableForAllPageTypes,flexipage:availableForRecordHome,force:hasRecordId,forceCommunity:availableForAllPageTypes,force:lightningQuickAction" >
    
    <!-- PJAIN: 20200330: Added below line to enable multi-line toast -->
    <aura:html tag="style">.toastMessage.forceActionsText{white-space : pre-line !important;}</aura:html>
    
    <!--Handlers-->
    <aura:handler action="{!c.doInit}" name="init" value="{!this}" />
     
    <lightning:workspaceAPI aura:id="workspace"/>
    
    <!--Attributes-->
    <aura:attribute type="List" name="docList" default=""/>
    <aura:attribute name="data" type="Object"/>
    <aura:attribute name="columns" type="List"/>
    <aura:attribute name="selectedCommMethod" type="String" />
    <aura:attribute name="activeSections" type="List" default="['Consent','Golden Source Documents']" />
    <aura:attribute name="defaultActiveSections" type="List" default="['Consent','Golden Source Documents','Account','IDV','commmunication','BusinessInfo','BusinessAdd','Financial','Regulatory','3IF','riskIndicator','fatca','LstDetails','OperDetails','Relationships', 'sicCodeClassification', 'tradingAsName']" /> <!-- PJAIN: 20200327 -->
    <aura:attribute name="accRecordId" type="String" />
    <aura:attribute name="businessProspectRecordTypeId" type="Id"/> <!-- PJAIN: 20200327 -->
    <aura:Attribute name="showRecordEditForm" type="boolean" default="false"/> <!-- PJAIN: 20200327 -->
    
    <aura:attribute name="isExperianModalOpen" type="boolean" default="false"/>
    <aura:attribute name="experianResponse" type="ExperianBusinessDetailsGet_Resp_IB_v1"/>
    <aura:attribute name="companyDetails" type="object"/>
    <aura:attribute name="registrationNumber" type="String"/>
    <aura:Attribute name="errorMessage" type="String" />
    <aura:Attribute name="isshowError" type="boolean" />
    <aura:Attribute name="proceedToOpp" type="boolean" default="false"/>
    <aura:attribute type="Boolean" name="showNatureOfBusinessModal" default="false" />
    <aura:attribute name="ProcessName" type="String" default=""/> <!-- 20200312: Prashant Jain and Tinashe Shoko: Added for Merchant Onboarding -->
    <aura:attribute name="processType" type="String" default=""/> <!-- TdB: Defines the Process Type (New to Bank/Lite Onboarding) -->
    <aura:Attribute name="goldenSourceConsentGiven" type="boolean" default="true"/>
    <aura:Attribute name="disableConsentCheckbox" type="boolean" default="true"/>	<!--Manoj 07022020 -->
    <aura:attribute type="Boolean" name="showLimitedAccountInfoModal" default="false" />
    <aura:attribute name="accountRecord" type="Account" default="{sobjectType:'Account'}"/>
    <aura:attribute name="goldenSourceMappings" type="List" description="This contains the mappings for consent goldensources" />
    <aura:attribute type="Boolean" name="showFsp" default="false" /><!--Added By Mbuyiseni Mbhokane-->
    <aura:attribute type="Boolean" name="showCondition" default="false" /><!--Added By Mbuyiseni Mbhokane-->
    <aura:attribute type="Boolean" name="showNamedCondition" default="false" /><!--Added By Mbuyiseni Mbhokane-->
    <aura:Attribute name="isAgreedPrivacypolicy" type="boolean" default="false"/><!--Added By Haritha Police-->
    <aura:Attribute name="isAgreedVerificationPolicy" type="boolean" default="false"/><!-- Muvhuso: SPM onboarding -->

    
    <!-- DESIGN ATTRIBUTES -->
    <aura:attribute name="flowAccountRecId" type="String" access="global"/>
    <aura:attribute name="account" type="Account" default="{sobjectType:'Account'}"/>
    <aura:attribute name="countriesTradedOptions" type="List"/>
    <aura:attribute name="countriesTradedValues" type="List"/>
    <aura:attribute type="Boolean" name="showNoTaxReason" default="false" />
    <aura:attribute type="Boolean" name="showReason" default="false" />
    <aura:attribute name="highIndustryValue" type="String"/>
    <aura:Attribute name="sicCategoryValue" type="String" />
    
    <aura:attribute name="clientType" type="String" />
    <aura:attribute name="clientType2" type="String" />
    <aura:attribute name="clientGroup" type="String" />
    <aura:attribute name="placeOfResidence" type="String" />
    <aura:attribute name="opportunityRecordId" type="String" /><!--Added By Masechaba Maseli to load Edit-->
    
    <aura:attribute name="showRegulatoryQ1SubSection" type="boolean" default="false"/>
    <aura:attribute name="showRegulatoryQ2SubSection" type="boolean" default="false"/>
    <aura:attribute name="showRegulatoryQ3SubSection" type="boolean" default="false"/>
    <aura:attribute name="showRegulatoryQ4SubSection" type="boolean" default="false"/>
    <aura:attribute name="showRegulatoryQ1SubSection2" type="boolean" default="false"/>
    <aura:attribute name="showRegulatoryQ2SubSection2" type="boolean" default="false"/>
    <aura:attribute name="showRegulatoryQ3SubSection2" type="boolean" default="false"/>
    <aura:attribute name="showRegulatoryQ4SubSection2" type="boolean" default="false"/>
    <aura:attribute name="showProvideDetails" type="boolean" default="false"/><!--added by Mashudu for SPM Onboarding -->
    
    <!--SPM attributes Added by Diksha 9/21/2020-->
    <aura:attribute name="isModalOpenforPM" type="boolean" default="false"/> <!-- Diksha for SPM selection of PM-->
    <aura:attribute name="selectedPm" type="string" />
    <aura:attribute name="pmList" type="User[]" />
    <aura:attribute name="loggedinUserProfile" type="string" />
    
    <aura:attribute name="showConsentSection" type="boolean" default="true"/> <!--6097 - Trigger Full Client On-boarding Prior Fulfilment -->
    <aura:attribute name="processNameP" type="String" /> <!--6097 - Trigger Full Client On-boarding Prior Fulfilment -->
    <aura:attribute name="liteOpportunityId" type="String" /> <!--6097 - Trigger Full Client On-boarding Prior Fulfilment -->
    <aura:attribute name="allselectedroles" type="String" />
    <aura:attribute name="previouslyClosedChecked" type="Boolean" default="false"/>
 	
    <aura:attribute name="showSave" type="Boolean" default="true"/> <!-- W-010629 show save button value passed form continue on-boarding from account-->
    <aura:attribute name="registerdChecked" type="Boolean" default="true"/>
    <aura:if isTrue="{!v.showSpinner}">
        <div class="slds-spinner_container">
            <lightning:spinner aura:id="TheSpinner" variant="brand" size="medium"/>
        </div>
    </aura:if>
    <center style="z-index:99">
        <lightning:spinner aura:id="TheSpinner" class="slds-hide" variant="brand" size="large"/>
    </center>
    <aura:if isTrue="{!v.isExperianModalOpen}">
        <section role="dialog" tabindex="-1" aria-labelledby="modal-heading-01" aria-modal="true" aria-describedby="modal-content-id-1" class="slds-modal slds-fade-in-open">
            
            <!--MODEL POPUP TO SHOW SUMMARY INFO FROM EXPERIAN -->
            <div class="slds-modal__container">
                <header class="slds-modal__header">
                    <button class="slds-button slds-button_icon slds-modal__close slds-button_icon-inverse" title="Close">
                        <span class="slds-assistive-text">Close</span>
                    </button>
                    <h2 id="modal-heading-01" class="slds-modal__title slds-hyphenate">Golden Sources Preview</h2>
                </header>
                <div class="slds-modal__content slds-p-around_medium" id="modal-content-id-1">
                    <div class="slds-grid slds-wrap slds-gutters">
                        <div class="slds-col slds-size_1-of-2 slds-p-bottom_medium">
                            <span>Name</span><br /><span>{!v.companyDetails.companyName}</span>
                        </div>
                        <div class="slds-col slds-size_1-of-2 slds-p-bottom_medium">
                            <span>Registration/ID Number</span><br /><span>{!v.companyDetails.registrationNumber}</span>
                        </div><br /><br />
                        <div class="slds-col slds-size_1-of-2 slds-p-bottom_medium">
                            <span>Entity Type</span><br /><span>{!v.companyDetails.entity}</span>
                        </div>
                        <div class="slds-col slds-size_1-of-2 slds-p-bottom_medium">
                            <span>Status</span><br /><span>{!v.companyDetails.status}</span>
                        </div>
                    </div>
                </div>
                <footer class="slds-modal__footer slds-modal__footer_directional">
                    
                    <lightning:button variant="neutral" class=""
                                      label="Incorrect Info? Try searching again." title="Save" aura:id="incorrectInfo"
                                      onclick="{!c.closeExperianModal}" />
                    <lightning:button variant="brand" class=""
                                      label="This info is correct. Proceed" title="Save" aura:id="correctInfo"
                                      onclick="{!c.proceedExperian}" />
                    
                </footer>
            </div>
        </section>
        <div class="slds-backdrop slds-backdrop_open"></div>
    </aura:if>
    
    <!-- TdB - Popup for no capture manually to cpature required minimal required details -->
    <aura:if isTrue="{!v.showLimitedAccountInfoModal}">
        <section role="dialog" tabindex="-1" aria-labelledby="modal-heading-01" aria-modal="true" aria-describedby="modal-content-id-1" class="slds-modal slds-fade-in-open">
            
            <!--MODEL POPUP TO SHOW SUMMARY INFO FROM EXPERIAN -->
            <div class="slds-modal__container">
                <header class="slds-modal__header">
                    <button class="slds-button slds-button_icon slds-modal__close slds-button_icon-inverse" title="Close">
                        <span class="slds-assistive-text">Close</span>
                    </button>
                    <h2 id="modal-heading-01" class="slds-modal__title slds-hyphenate">Business Onboarding</h2>
                </header>
                <div class="slds-modal__content slds-is-expanded slds-p-around_medium" id="modal-content-id-1">
                    <div class="slds-grid slds-wrap slds-gutters ">
                        <lightning:recordEditForm aura:id="recordNewAccount"
                                                  recordId="{!v.accRecordId}"
                                                  recordTypeId="{!v.businessProspectRecordTypeId}"
                                                  objectApiName="Account">
                            <lightning:layout multipleRows="true">
                                <aura:if isTrue="{!or(or(or(or(v.accountRecord.Client_Type__c == 'Clubs/Societies/Associations/Other Informal Bodies', v.accountRecord.Client_Type__c == 'Central Bank or Regulator'),v.accountRecord.Client_Type__c == 'Non Profit Organizations (NGO)'),v.accountRecord.Client_Type__c == 'Schools'),v.accountRecord.Client_Type__c == 'Foreign Trust')}">
                                    <lightning:layoutItem padding="" size="6" smallDeviceSize="12" mediumDeviceSize="6">
                                        <lightning:inputField fieldName="Name" aura:id="accountName" label="Account Name" required="true" value="{!v.accountRecord.Registration_Number__c}"/>
                                    </lightning:layoutItem>
                                    <aura:set attribute="else">
                                        <lightning:layoutItem padding="" size="6" smallDeviceSize="12" mediumDeviceSize="6">
                                            <lightning:inputField fieldName="Name" aura:id="accountName" label="Account Name" required="true" value="{!v.accountRecord.Name}"/>
                                        </lightning:layoutItem>
                                    </aura:set>
                                </aura:if>
                                <lightning:layoutItem padding="" size="6" smallDeviceSize="12" mediumDeviceSize="6">
                                    <lightning:inputField fieldName="Client_Group__c" aura:id="clientGroup"  disabled="true" value="{!v.accountRecord.Client_Group__c}"/>
                                    <div aura:id="errclientGroup" class="slds-hide" style="color: var(--lwc-colorTextError,rgb(194, 57, 52)) !important;">Please Enter Client Group as Non Individual</div>
                                </lightning:layoutItem>
                                <aura:if isTrue="{!v.accountRecord.Client_Type__c !='Trusts'}">
                                    <aura:if isTrue="{!or(or(or(or(v.accountRecord.Client_Type__c == 'Clubs/Societies/Associations/Other Informal Bodies', v.accountRecord.Client_Type__c == 'Central Bank or Regulator'),v.accountRecord.Client_Type__c == 'Non Profit Organizations (NGO)'),v.accountRecord.Client_Type__c == 'Schools'),v.accountRecord.Client_Type__c == 'Foreign Trust')}">
                                <lightning:layoutItem padding="" size="6" smallDeviceSize="12" mediumDeviceSize="6" >
                                            <lightning:inputField fieldName="Registration_Number__c" aura:id="regNumber" label="Registration Number / ID Number" value="{!v.accountRecord.Name}"/>
                                        </lightning:layoutItem>
                                        <aura:set attribute="else">
                                            <lightning:layoutItem padding="" size="6" smallDeviceSize="12" mediumDeviceSize="6" >
                                                <lightning:inputField fieldName="Registration_Number__c" aura:id="regNumber" label="Registration Number / ID Number" value="{!v.accountRecord.Registration_Number__c}"/>
                                            </lightning:layoutItem>
                                        </aura:set>
                                    </aura:if>
                                    <aura:set attribute="else">
                                        <lightning:layoutItem padding="" size="6" smallDeviceSize="12" mediumDeviceSize="6">
                                            <lightning:inputField fieldName="Trust_Number__c" aura:id="trustNumber" required="true" value="{!v.accountRecord.Trust_Number__c}" />
                                            <div aura:id="errtrustNumber" class="slds-hide" style="color: var(--lwc-colorTextError,rgb(194, 57, 52)) !important;">Please Enter Trust Number</div>
                                        </lightning:layoutItem>
                                    </aura:set>
                                </aura:if>
                                <lightning:layoutItem padding="" size="6" smallDeviceSize="12" mediumDeviceSize="6">
                                    <lightning:inputField fieldName="Client_Type__c" aura:id="idClientType" disabled="true" value="{!v.accountRecord.Client_Type__c}" />
                                    <div aura:id="errclientType" class="slds-hide" style="color: var(--lwc-colorTextError,rgb(194, 57, 52)) !important;">Please Enter Client Type as Private Company</div>
                                </lightning:layoutItem>
                                <aura:if isTrue="{!v.accountRecord.Client_Type__c !='Trusts'}">
                                    <lightning:layoutItem padding="" size="6" smallDeviceSize="12" mediumDeviceSize="6">
                                        <lightning:inputField fieldName="ID_Type__c" aura:id="idIdType" required="true" value="{!v.accountRecord.ID_Type__c}" />
                                        <div aura:id="errIdType" class="slds-hide" style="color: var(--lwc-colorTextError,rgb(194, 57, 52)) !important;">Please Enter Id Type</div>
                                    </lightning:layoutItem>
                                    <aura:set attribute="else">
                                        <lightning:layoutItem padding="" size="6" smallDeviceSize="12" mediumDeviceSize="6">
                                            <lightning:inputField fieldName="Trust_Number_New_format__c" aura:id="trustNumberNewFormat"  value="{!v.accountRecord.Trust_Number_New_format__c}" />
                                            <div aura:id="errtrustNumberNewFormat" class="slds-hide" style="color: var(--lwc-colorTextError,rgb(194, 57, 52)) !important;">Please Enter Trust Number</div>
                                        </lightning:layoutItem>
                                    </aura:set>
                                </aura:if>
                                
                                <lightning:layoutItem padding="" size="6" smallDeviceSize="12" mediumDeviceSize="6">
                                    <lightning:inputField fieldName="Place_of_Residence__c" aura:id="placeOfResidence2" required="true" value="{!v.accountRecord.Place_of_Residence__c}" />
                                    <div aura:id="errPlaceOfResindence" class="slds-hide" style="color: var(--lwc-colorTextError,rgb(194, 57, 52)) !important;">Please select Place of Residence</div>
                                </lightning:layoutItem>
                            </lightning:layout>
                        </lightning:recordEditForm>
                    </div>
                    
                </div>
                <footer class="slds-modal__footer slds-modal__footer_directional">
                    
                    <lightning:button variant="neutral" class=""
                                      label="Cancel" title="Cancel" aura:id="cancelInfo"
                                      onclick="{!c.closeLimitedAccDetailsModal}" />
                    <lightning:button variant="brand" class=""
                                      label="Proceed to Onboarding" title="Save" aura:id="saveInfo"
                                      onclick="{!c.showBigForm}" />
                    
                </footer>
            </div>
        </section>
        <div class="slds-backdrop slds-backdrop_open"></div>
    </aura:if>
    
    <!--<c:onboardingProgressIndicator currentStep="Complete Client Details" />-->
    <lightning:accordion activeSectionName="{! v.activeSections }" allowMultipleSectionsOpen="true">
        <h1 class="slds-section__title">
            <span class="slds-align_absolute-center" title="Section Title">New-To-Bank Onboarding</span>
        </h1>
        <aura:if isTrue="{!v.showConsentSection}">
            <!--CONSENT INFORMATION -->
            <lightning:accordionSection name="Consent" label="Consent Declaration for Client and Related Parties">
                
                <!--
                <div class="slds-form-element" aura:id="regNumberError">
                    <div class="slds-form-element__control">
                        <lightning:input disabled="{!v.disableConsentCheckbox}" name="regNumberC" aura:id="regNumberC" label="Registration/ID Number" placeholder="Type here..." value="{!v.registrationNumber}"/>
                    </div>
                    <div class="slds-form-element__help slds-hide" id="form-error-01 ">This field is required</div>
                </div>
                -->
                
                <!--W-005633 - Manoj 09152020 - New Design for Prospect-START -->
                <lightning:recordEditForm aura:id="recordNewAccount"
                                          recordId="{!v.accRecordId}"
                                          recordTypeId="{!v.businessProspectRecordTypeId}"
                                          objectApiName="Account">
                    <div class="{!if(v.accountRecord.Client_Type__c != 'Trusts', '', 'display:none')}"  aura:id="regNumberError">
                        <div class="slds-form-element__control">
                            <lightning:input name="regNumberC" aura:id="regNumberC" label="Registration/ID Number" placeholder="Type here..." value="{!v.registrationNumber}"/>
                        </div>
                        <div class="slds-form-element__help slds-hide" id="form-error-01 ">This field is required</div>
                    </div>
                    <!--
                    <lightning:layout multipleRows="true">
                        <lightning:layoutItem padding="" size="6" smallDeviceSize="12" mediumDeviceSize="6">
                            <lightning:inputField fieldName="Client_Group__c" aura:id="clientGroup2"  required="true" value="{!v.accountRecord.Client_Group__c}"/>
                            <div aura:id="errclientGroup" class="slds-hide" style="color: var(-lwc-colorTextError,rgb(194, 57, 52)) !important;">Please select Client Group as Non Individual</div>
                        </lightning:layoutItem>
                        <lightning:layoutItem padding="" size="6" smallDeviceSize="12" mediumDeviceSize="6">
                            <lightning:inputField fieldName="Client_Type__c" aura:id="idClientType2" required="true" value="{!v.accountRecord.Client_Type__c}" onchange="{!c.handleConsent}"/>
                            <div aura:id="errclientType" class="slds-hide" style="color: var(-lwc-colorTextError,rgb(194, 57, 52)) !important;">Please select Client Type as Private Company</div>
                        </lightning:layoutItem>
                        <lightning:layoutItem padding="" size="6" smallDeviceSize="12" mediumDeviceSize="6">
                            <lightning:inputField fieldName="Place_of_Residence__c" aura:id="placeOfResidence" required="true" value="{!v.accountRecord.Place_of_Residence__c}" />
                            <div aura:id="errPlaceOfResindence" class="slds-hide" style="color: var(-lwc-colorTextError,rgb(194, 57, 52)) !important;">Please select Place of Residence</div>
                        </lightning:layoutItem>
                        
                        <lightning:layoutItem padding="" size="6" smallDeviceSize="12" mediumDeviceSize="6" >                        
                            <div style="{!v.accountRecord.Client_Type__c != 'Trusts' ? '': 'display:none;'}" class="slds-form-element" aura:id="regNumberError">
                                <div class="slds-form-element__control">
                                    <lightning:input name="regNumberC" aura:id="regNumberC" label="Registration/ID Number" placeholder="Type here..." value="{!v.registrationNumber}"/>
                                </div>
                                <div class="slds-form-element__help slds-hide" id="form-error-01 ">This field is required</div>
                            </div>
                        </lightning:layoutItem>
                    </lightning:layout>
                    -->
                    <!--W-005633 - Manoj 09152020 - New Design for Business Prospect-END -->            
                    
                    <lightning:input disabled="{!v.disableConsentCheckbox}" type="checkbox" label="I AGREE TO ABSA BANK ACCESSING MY RECORDS ON GOLDEN SOURCES" name="Agree Golden Sources"
                                     onclick="{!c.showPopulateDetailsButton}" aura:id="agreed" />
                    <!--W-005816 - Haritha 10092020 - Consent for spm Onboarding -->   
                    <aura:if isTrue="{!v.ProcessName == 'SPMOnboarding'}">
                        <lightning:input  type="checkbox" label="ABSA MAY SHARE INFORMATION REGARDING MY APPLICATION WITH THIRD PARTIES IN ACCORDANCE WITH THE PRIVACY POLICY" name="AgreePrivacyPolicy"
                                         onchange="{!c.savecheckedaccount}" aura:id="AgreePrivacyPolicy" />
                        <!--  <lightning:input type="checkbox" name='AgreePrivacyPolicy' aura:id="checkbox" label="ABSA MAY SHARE INFORMATION REGARDING MY APPLICATION WITH THIRD PARTIES IN ACCORDANCE WITH THE PRIVACY POLICY" change="{!c.onChange}"/>-->
                         <!--W-007037 - Muvhuso 03112021 - Consent for spm Onboarding -->
                        <lightning:input type="checkbox" label="I GIVE ABSA PERMISSION TO SOURCE AND USE MY DETAILS FOR IDENTIFICATION AND VERIFICATION PURPOSES IN ACCORDANCE WITH THE FINANCIAL INTELLIGENCE CENTRE ACT AND OTHER APPLICABLE LEGISLATION." name="AgreePrivacyPolicy"
                                 onchange="{!c.savecheckedAccountConsent}" aura:id="AgreeVerificatonPolicy" />
                    </aura:if>
                    <div class="c-container">
                        <lightning:layout multipleRows="true">
                            <lightning:layoutItem padding="around-small" size="12" class="slds-align_absolute-center">
                                <!-- <div class="slds-hide" aura:id="populateDiv"> -->
                                <lightning:button disabled="{!v.goldenSourceConsentGiven}" variant="brand" class="slds-m-top_xx-large" label="Populate Details from Golden Sources"
                                                  title="Populate Details from Golden Sources" onclick="{! c.populateDetails }"/>
                                <lightning:button disabled="{!not(v.goldenSourceConsentGiven)}" variant="brand" class=" slds-m-top_xx-large" label="Capture Information Manually"
                                                  title="No Consent Given" onclick="{! c.openLimitedAccDetailsModal }"/>
                                <!-- </div> -->
                            </lightning:layoutItem>
                        </lightning:layout>
                    </div>  
                </lightning:recordEditForm>
            </lightning:accordionSection>
        </aura:if>
        
        <aura:if isTrue="{!v.showRecordEditForm}"> <!-- PJAIN: 20200327 -->
            <div class="slds-hide" aura:id="goldenSourceDocs">
                <lightning:recordEditForm aura:id="recordViewForm22"
                                          recordId="{!v.accRecordId}"
                                          recordTypeId="{!v.businessProspectRecordTypeId}"
                                          objectApiName="Account"
                                          onsubmit="{!c.handleOnSubmit}"
                                          onsuccess="{!c.handleOnSuccess}"
                                          onerror="{!c.handleOnError}"
                                          onload="{!c.handleOnLoad}">
                    
                    <!--W-010629-->
                    <aura:if isTrue="{!and(v.showSave, v.accRecordId!= null)}">
                        <div class="slds-align_absolute-center">
                            <lightning:button type="submit" variant="brand" label="Save and Continue " onclick="{!c.onSave}"/>
                        </div>
                    </aura:if>
                    <!--GLODEN SOURCE DOCUMENTS START-->
                    <!--
<lightning:accordionSection name="Golden Source Documents" label="Golden Source Documents">
<lightning:datatable
keyField=""
data="{! v.docList }"
columns="{! v.columns }"
hideCheckboxColumn="true"/>
</lightning:accordionSection>
-->
                    <!--GLODEN SOURCE DOCUMENTS END-->
                    <br />
                    
                    <!--ACCOUNT INFORMATION START-->
                    <lightning:accordionSection name="Account" label="Account Information">
                        
                        <div class="slds-grid slds-wrap slds-gutters">
                            <lightning:layout multipleRows="true">      
                                <lightning:layoutItem padding="" size="6" smallDeviceSize="12" mediumDeviceSize="6">
                                    <lightning:inputField fieldName="Name" aura:id="accountName" label="Account Name" required="true"/>
                                </lightning:layoutItem>
                                
                                <!--TdB - Show/Hide fields based on New to Bank/Lite Onboarding -->
                                <aura:if isTrue="{!v.processType != 'Lite Onboarding'}">
                                    <aura:if isTrue="{!v.accountRecord.Client_Type__c !='Joint &amp; Several'}">
                                        <lightning:layoutItem padding="" size="6" smallDeviceSize="12" mediumDeviceSize="6">
                                            <lightning:input aura:id="entityType" label="Entity Name" required="true" value="{!v.clientGroup +'-'+ v.clientType+ '-'+ v.placeOfResidence}"/>
                                        </lightning:layoutItem>
                                    </aura:if>
                                </aura:if>
                                <lightning:layoutItem padding="" size="6" smallDeviceSize="12" mediumDeviceSize="6">
                                    <lightning:inputField fieldName="Client_Type__c" aura:id="clientType" disabled="true"  onchange="{!c.setClientTypeValue}"/>
                                    <div aura:id="errclientType" class="slds-hide" style="color: var(--lwc-colorTextError,rgb(194, 57, 52)) !important;">Please Enter Client Type as Private Company</div>
                                </lightning:layoutItem>
                                <lightning:layoutItem padding="" size="6" smallDeviceSize="12" mediumDeviceSize="6">
                                    <lightning:inputField fieldName="ID_Type__c" aura:id="idType" required="true"/>
                                    <div aura:id="erridType" class="slds-hide" style="color: var(--lwc-colorTextError,rgb(194, 57, 52)) !important;">Please Enter ID Type</div>
                                </lightning:layoutItem>  
                                <aura:if isTrue="{!and(v.clientType2 != 'Trusts',v.clientType2 != 'Foreign Trust')}">    
                                    <aura:set attribute="else">
                                        <lightning:layoutItem padding="" size="6" smallDeviceSize="12" mediumDeviceSize="6">
                                            <lightning:inputField fieldName="Trust_Number__c" aura:id="trustNumBigForm"/>
                                        </lightning:layoutItem>
