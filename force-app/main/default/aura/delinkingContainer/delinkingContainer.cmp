<aura:component implements="flexipage:availableForAllPageTypes" access="global">

    <aura:attribute name="accountId" type="String" access="global"/>
    <aura:attribute name="clientKey" type="String" access="global"/>
    <aura:attribute name="idNumber" type="String" access="global"/>
    <aura:attribute name="useBiometrics" type="Boolean" access="global"/>

    <aura:handler name="init" value="{!this}" action="{!c.doInit}"/>

    <lightning:flow aura:id="delinkingFlow"/>
</aura:component>