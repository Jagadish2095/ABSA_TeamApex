<!--
* Lightning Component for Onboarding Client Information (OnboardingClientDetails)
*
* @author  Manoj Danthala : Absa
* @version v1.0
* @since   2019-11-20
*
-->
<aura:component access="global" controller="OnboardingClientDetailsController"
                implements="lightning:isUrlAddressable,lightning:availableForFlowScreens,force:appHostable,flexipage:availableForAllPageTypes,flexipage:availableForRecordHome,force:hasRecordId,forceCommunity:availableForAllPageTypes,force:lightningQuickAction,lightning:actionOverride" >
    
    <!-- PJAIN: 20200330: Added below line to enable multi-line toast -->
    <aura:html tag="style">.toastMessage.forceActionsText{white-space : pre-line !important;}</aura:html>
    
    <!--Handlers-->
    <aura:handler action="{!c.doInit}" name="init" value="{!this}" />
    
    <lightning:workspaceAPI aura:id="workspace"/>
    
    <!--Attributes-->
    <aura:attribute name="recordId" type="String" access="global"/>
    <aura:attribute type="List" name="docList" default=""/>
    <aura:attribute name="data" type="Object"/>
    <aura:attribute name="columns" type="List"/>
    <aura:attribute name="selectedCommMethod" type="String" />
    <aura:attribute name="activeSections" type="List" default="['Consent','Golden Source Documents']" />
    <aura:attribute name="defaultActiveSections" type="List" default="['Consent','Golden Source Documents','Account','IDV','commmunication','BusinessInfo','BusinessAdd','Financial','Regulatory','3IF','riskIndicator','fatca','Relationships', 'sicCodeClassification', 'tradingAsName']" /> <!-- PJAIN: 20200327 -->
    <aura:attribute name="accRecordId" type="String"  />
    <aura:attribute name="businessProspectRecordTypeId" type="Id"/> <!-- PJAIN: 20200327 -->
    <aura:Attribute name="showRecordEditForm" type="boolean" default="false"/> <!-- PJAIN: 20200327 -->
    	
    <aura:attribute name="isExperianModalOpen" type="boolean" default="false"/>
    <aura:attribute name="experianResponse" type="ExperianBusinessDetailsGet_Resp_IB_v1"/>
    <aura:attribute name="companyDetails" type="object"/>
    <aura:attribute name="registrationNumber" type="String"/>
    <aura:Attribute name="errorMessage" type="String" />
    <aura:Attribute name="isshowError" type="boolean" />
    <aura:Attribute name="proceedToOpp" type="boolean" default="false"/>
    <aura:attribute type="Boolean" name="showNatureOfBusinessModal" default="false" />
    <aura:attribute name="ProcessName" type="String" default=""/> <!-- 20200312: Prashant Jain and Tinashe Shoko: Added for Merchant Onboarding -->
    <aura:Attribute name="goldenSourceConsentGiven" type="boolean" default="true"/>
    <aura:Attribute name="disableConsentCheckbox" type="boolean" default="true"/>	<!--Manoj 07022020 -->
    <aura:attribute type="Boolean" name="showLimitedAccountInfoModal" default="false" />
    <aura:attribute name="accountRecord" type="Account" default="{sobjectType:'Account'}"/>
    <aura:attribute name="goldenSourceMappings" type="List" description="This contains the mappings for consent goldensources" />

	<aura:attribute type="Boolean" name="showFsp" default="false" /><!--Added By Mbuyiseni Mbhokane-->
    <aura:attribute type="Boolean" name="showCondition" default="false" /><!--Added By Mbuyiseni Mbhokane-->
    <aura:attribute type="Boolean" name="showNamedCondition" default="false" /><!--Added By Mbuyiseni Mbhokane-->
    <aura:Attribute name="isAgreedPrivacypolicy" type="boolean" default="true"/><!--Added By Haritha Police-->    
    
    <!-- DESIGN ATTRIBUTES -->
    <aura:attribute name="flowAccountRecId" type="String" access="global"/>
    <aura:attribute name="account" type="Account" default="{sobjectType:'Account'}"/>
    <aura:attribute name="countriesTradedOptions" type="List"/>
    <aura:attribute name="countriesTradedValues" type="List"/>
    <aura:attribute type="Boolean" name="showNoTaxReason" default="false" />
    <aura:attribute type="Boolean" name="showReason" default="false" />
    <aura:attribute name="highIndustryValue" type="String"/>
    <aura:Attribute name="sicCategoryValue" type="String" />
    
    <aura:attribute name="clientType" type="String" />
    <aura:attribute name="clientType2" type="String" />
    <aura:attribute name="clientGroup" type="String" />
    <aura:attribute name="placeOfResidence" type="String" />
    
    <aura:attribute name="showRegulatoryQ1SubSection" type="boolean" default="false"/>
    <aura:attribute name="showRegulatoryQ2SubSection" type="boolean" default="false"/>
    <aura:attribute name="showRegulatoryQ3SubSection" type="boolean" default="false"/>
    <aura:attribute name="showRegulatoryQ4SubSection" type="boolean" default="false"/>
    <aura:attribute name="showRegulatoryQ1SubSection2" type="boolean" default="false"/>
    <aura:attribute name="showRegulatoryQ2SubSection2" type="boolean" default="false"/>
    <aura:attribute name="showRegulatoryQ3SubSection2" type="boolean" default="false"/>
    <aura:attribute name="showRegulatoryQ4SubSection2" type="boolean" default="false"/>
    <aura:attribute name="activeSectionsMessage" type="String" default="" />
    
       <!--Attributes-->
       <!--SPM attributes Added by Diksha 9/21/2020-->
    <aura:attribute name="isModalOpenforPM" type="boolean" default="false"/> <!-- Diksha for SPM selection of PM-->
    <aura:attribute name="selectedPm" type="string" />
    <aura:attribute name="pmList" type="User[]" />
    <aura:attribute name="loggedinUserProfile" type="string" />
    
    <aura:attribute name="showConsentSection" type="boolean" default="true"/> <!--6097 - Trigger Full Client On-boarding Prior Fulfilment -->
    <aura:attribute name="processNameP" type="String" /> <!--6097 - Trigger Full Client On-boarding Prior Fulfilment -->
    <aura:attribute name="liteOpportunityId" type="String" /> <!--6097 - Trigger Full Client On-boarding Prior Fulfilment -->
    <aura:attribute name="allselectedroles" type="String" />
   <aura:attribute name="accountRecord2" type="Object"/>
    <aura:attribute name="recordLoadError" type="String"/>
        <force:recordData aura:id="recordLoader"
                      recordId="{!v.recordId}"
                      fields="Client_Type__c"
                      targetFields="{!v.accountRecord2}"
                      targetError="{!v.recordLoadError}"
                      />
    
    <aura:if isTrue="{!v.showSpinner}">
        <div class="slds-spinner_container">
            <lightning:spinner aura:id="TheSpinner" variant="brand" size="medium"/>
        </div>
    </aura:if>
    <center style="z-index:99">
        <lightning:spinner aura:id="TheSpinner" class="slds-hide" variant="brand" size="large"/>
    </center>

    <!--Golden Source opoup preview-->
    <aura:if isTrue="{!v.isConfirmationModalOpen}">
        <section role="dialog" tabindex="-1" aria-labelledby="modal-heading-01" aria-modal="true" aria-describedby="modal-content-id-1" class="slds-modal slds-fade-in-open">
            <div class="slds-modal__container">
                <header class="slds-modal__header">
                    <button class="slds-button slds-button_icon slds-modal__close slds-button_icon-inverse" title="Close">
                        <span class="slds-assistive-text">Close</span>
                    </button>
                    <h2 id="modal-heading-01" class="slds-modal__title slds-hyphenate">Golden Sources Preview</h2>
                </header>
                <div class="slds-modal__content slds-p-around_large" id="modal-content-id-1">
                    <div class="slds-grid slds-wrap slds-gutters">
                        <div class="slds-col slds-size_1-of-2">
                            <span>First Name</span><br /><span>{!v.hanisResponse.name}</span><br /><br />
                        </div>
                        <div class="slds-col slds-size_1-of-2">
                            <span>ID Number</span><br /><span>{!v.hanisResponse.idNumber}</span><br /><br />
                        </div>
                        <div class="slds-col slds-size_1-of-2">
                            <span>Surname</span><br /><span>{!v.hanisResponse.surname}</span>
                        </div>
                        <div class="slds-col slds-size_1-of-2">
                            <span>Address</span><br /><span>{!if(v.residentialAddressDetails != null, v.residentialAddressDetails.FullAddress, null)}</span>
                        </div>
                    </div>
                </div>
                <footer class="slds-modal__footer slds-modal__footer_directional">
                    
                    <lightning:button variant="neutral"
                                      label="Incorrect Info? Try searching again." title="Save"
                                      onclick="{!c.closeAccountPreviewModal}"/>
                    <lightning:button variant="brand"
                                      label="This info is correct. Proceed" title="Save"
                                      onclick="{!c.createIndividualAccount}"/>
                </footer>
            </div>
        </section>
        <div class="slds-backdrop slds-backdrop_open"></div>
    </aura:if>
    
    <!-- TdB - Popup for no capture manually to cpature required minimal required details -->
    <aura:if isTrue="{!v.clientType == 'Sole Trader'}">
    <!--<c:onboardingProgressIndicator currentStep="Complete Client Details" />-->
    <lightning:accordion activeSectionName="{! v.activeSections }" allowMultipleSectionsOpen="true">
        
        <!--CONSENT INFORMATION -->
        <h1 class="slds-section__title">
            <span class="slds-align_absolute-center" title="Section Title">Client Edit</span>
        </h1>
        
            <div  aura:id="goldenSourceDocs" style="margin-bottom: 40px;">
                <br />
                
                <lightning:recordEditForm aura:id="recordViewForm22"
                                          recordId="{!v.recordId}"
                                          objectApiName="Account"
                                       
                                          onsuccess="{!c.handleOnSuccess}"
                                          recordTypeId="{!v.indvRecordTypeId}">
                    <!--ACCOUNT INFORMATION START-->
                    <lightning:accordionSection name="Account" label="Account Information">
                        <div class="slds-grid slds-wrap slds-gutters">
                            <lightning:layout multipleRows="true">
                                
                                <!--TdB - Show/Hide fields based on New to Bank/Lite Onboarding -->
                                <aura:if isTrue="{!v.processType != 'Lite Onboarding'}">
                                    <lightning:layoutItem padding="" size="6" smallDeviceSize="12" mediumDeviceSize="6">
                                        <lightning:inputField disabled="{!v.isFormReadOnly}" fieldName="PersonTitle" aura:id="iPersonTitle" required="true"/>
                                    </lightning:layoutItem>
                                    <lightning:layoutItem padding="" size="6" smallDeviceSize="12" mediumDeviceSize="6">
                                        <lightning:inputField disabled="{!v.isFormReadOnly}" fieldName="Initials__pc" aura:id="iInitials" required="true"/>
                                    </lightning:layoutItem>
                                </aura:if>
                                
                                <lightning:layoutItem padding="" size="6" smallDeviceSize="12" mediumDeviceSize="6">
                                    <lightning:inputField disabled="{!v.isFormReadOnly}" fieldName="FirstName" aura:id="iFirstName" label="Account Name" required="true"/>
                                    <div aura:id="errFirstName" class="slds-hide" style="color: var(--lwc-colorTextError,rgb(194, 57, 52)) !important;">Please Enter First Name</div>
                                </lightning:layoutItem>
                                <lightning:layoutItem padding="" size="6" smallDeviceSize="12" mediumDeviceSize="6">
                                    <lightning:inputField disabled="{!v.isFormReadOnly}" fieldName="LastName" aura:id="iLastName" label="Account Name" required="true"/>
                                    <div aura:id="errLastNam" class="slds-hide" style="color: var(--lwc-colorTextError,rgb(194, 57, 52)) !important;">Please Enter LastName</div>
                                </lightning:layoutItem>
                                <lightning:layoutItem padding="" size="6" smallDeviceSize="12" mediumDeviceSize="6">
                                    <lightning:inputField disabled="{!v.isFormReadOnly}" fieldName="ID_Type__c" aura:id="idType" required="true"/>
                                    <div aura:id="erridType" class="slds-hide" style="color: var(--lwc-colorTextError,rgb(194, 57, 52)) !important;">Please Enter ID Type</div>
                                </lightning:layoutItem>
                                
                                <!--TdB - Show/Hide fields based on New to Bank/Lite Onboarding -->
                                <aura:if isTrue="{!v.processType != 'Lite Onboarding'}">
                                    <lightning:layoutItem padding="" size="6" smallDeviceSize="12" mediumDeviceSize="6">
                                        <lightning:inputField disabled="{!v.isFormReadOnly}" fieldName="Primary_Identification_Reason__c" aura:id="identReason"/>
                                    </lightning:layoutItem>
                                    <lightning:layoutItem padding="" size="6" smallDeviceSize="12" mediumDeviceSize="6">
                                        <lightning:inputField disabled="{!v.isFormReadOnly}" fieldName="Client_Agreement__c" aura:id="iClientAgreement" required="true"/>
                                    </lightning:layoutItem>
                                </aura:if>
                                
                                <lightning:layoutItem padding="" size="6" smallDeviceSize="12" mediumDeviceSize="6" >
                                    <lightning:inputField disabled="{!v.isFormReadOnly}" fieldName="ID_Number__pc" aura:id="iIDNumber" label="ID Number" required="true"/>
                                    <div aura:id="errIDNumber" class="slds-hide" style="color: var(--lwc-colorTextError,rgb(194, 57, 52)) !important;">Please Enter ID Type</div>
                                </lightning:layoutItem>
                                
                                <!--TdB - Show/Hide fields based on New to Bank/Lite Onboarding -->
                                <aura:if isTrue="{!v.processType != 'Lite Onboarding'}">
                                    <lightning:layoutItem padding="" size="6" smallDeviceSize="12" mediumDeviceSize="6" >
                                        <lightning:inputField disabled="{!v.isFormReadOnly}" fieldName="Date_Issued__pc" aura:id="iDateIssued"/>
                                    </lightning:layoutItem>
                                    <lightning:layoutItem padding="" size="6" smallDeviceSize="12" mediumDeviceSize="6">
                                        <lightning:inputField disabled="{!v.isFormReadOnly}" fieldName="Gender__pc" aura:id="iGender" required="true"/>
                                    </lightning:layoutItem>
                                </aura:if>
                                
                                <lightning:layoutItem padding="" size="6" smallDeviceSize="12" mediumDeviceSize="6">
                                    <lightning:inputField disabled="{!v.isFormReadOnly}" fieldName="Client_Group__c" aura:id="iClientGroup" onchange="{!c.setSectionsToRender}" required="true"/>
                                    <div aura:id="errclientGroup" class="slds-hide" style="color: var(--lwc-colorTextError,rgb(194, 57, 52)) !important;">Please Enter Client Group as Non Individual</div>
                                </lightning:layoutItem>
                                <lightning:layoutItem padding="" size="6" smallDeviceSize="12" mediumDeviceSize="6">
                                    <lightning:inputField disabled="{!v.isFormReadOnly}" fieldName="Client_Type__c" aura:id="iClientType" onchange="{!c.setSectionsToRender}" required="true"/>
                                    <div aura:id="errclientType" class="slds-hide" style="color: var(--lwc-colorTextError,rgb(194, 57, 52)) !important;">Please Enter Client Type</div>
                                </lightning:layoutItem>
                                
                                <!--TdB - Show/Hide fields based on New to Bank/Lite Onboarding -->
                                <aura:if isTrue="{!v.processType != 'Lite Onboarding'}">
                                    <lightning:layoutItem padding="" size="6" smallDeviceSize="12" mediumDeviceSize="6">
                                        <lightning:inputField disabled="{!v.isFormReadOnly}" fieldName="Business_Unit__c" aura:id="iBusinessUnit" required="true"/>
                                        <div aura:id="errbusinessUnit" class="slds-hide" style="color: var(--lwc-colorTextError,rgb(194, 57, 52)) !important;">Please Enter Business Unit</div>
                                    </lightning:layoutItem>
                                </aura:if>
                                <lightning:layoutItem padding="" size="6" smallDeviceSize="12" mediumDeviceSize="6">
                                    <lightning:inputField disabled="{!v.isFormReadOnly}" fieldName="PersonBirthdate" aura:id="iPersonBirthdate" required="true"/>
                                    <div aura:id="errPersonBirthdate" class="slds-hide" style="color: var(--lwc-colorTextError,rgb(194, 57, 52)) !important;">Please Enter Birthdate</div>
                                </lightning:layoutItem>
                                
                                
                                <lightning:layoutItem padding="" size="6" smallDeviceSize="12" mediumDeviceSize="6">
                                    <lightning:inputField disabled="{!v.isFormReadOnly}" fieldName="Country_of_Birth__pc" aura:id="iCountryOfBirth" required="true"/>
                                    <div aura:id="errCountryOfBirth" class="slds-hide" style="color: var(--lwc-colorTextError,rgb(194, 57, 52)) !important;">Please Enter Country Of Birth</div>
                                </lightning:layoutItem>
                                
                                <!--TdB - Show/Hide fields based on New to Bank/Lite Onboarding -->
                                <aura:if isTrue="{!v.processType != 'Lite Onboarding'}">
                                    <lightning:layoutItem padding="" size="6" smallDeviceSize="12" mediumDeviceSize="6">
                                        <lightning:inputField disabled="{!v.isFormReadOnly}" fieldName="Passport_Number__pc" aura:id="iPassportNumber"/>
                                    </lightning:layoutItem>
                                    <lightning:layoutItem padding="" size="6" smallDeviceSize="12" mediumDeviceSize="6">
                                        <lightning:inputField disabled="{!v.isFormReadOnly}" fieldName="Country_Passport_Issued__pc" aura:id="iCountryPassportIssued"/>
                                    </lightning:layoutItem>
                                </aura:if>
                                
                                <lightning:layoutItem padding="" size="6" smallDeviceSize="12" mediumDeviceSize="6" >
                                    <lightning:inputField disabled="{!v.isFormReadOnly}" fieldName="Marital_Status__pc" aura:id="iMaritalStatus" required="true"/>
                                </lightning:layoutItem>
                                <lightning:layoutItem padding="" size="6" smallDeviceSize="12" mediumDeviceSize="6">
                                    <lightning:inputField disabled="{!v.isFormReadOnly}" fieldName="Marital_Contract_Type__pc" aura:id="iMaritalContractType" required="true"/>
                                </lightning:layoutItem>
                                
                                <!--TdB - Show/Hide fields based on New to Bank/Lite Onboarding -->
                                <aura:if isTrue="{!v.processType != 'Lite Onboarding'}">
                                    <lightning:layoutItem padding="" size="6" smallDeviceSize="12" mediumDeviceSize="6">
                                        <lightning:inputField disabled="{!v.isFormReadOnly}" fieldName="PersonEmail" aura:id="iEmail" />
                                    </lightning:layoutItem>
                                    <lightning:layoutItem padding="" size="6" smallDeviceSize="12" mediumDeviceSize="6">
                                        <lightning:inputField disabled="{!v.isFormReadOnly}" fieldName="PersonMobilePhone" aura:id="iPersonMobilePhone" />
                                    </lightning:layoutItem>
                                    <lightning:layoutItem padding="" size="6" smallDeviceSize="12" mediumDeviceSize="6">
                                        <lightning:inputField disabled="{!v.isFormReadOnly}" fieldName="PersonHomePhone" aura:id="iPersonHomePhone" />
                                    </lightning:layoutItem>   
                                    <lightning:layoutItem padding="" size="6" smallDeviceSize="12" mediumDeviceSize="6">
                                        <lightning:inputField disabled="{!v.isFormReadOnly}" fieldName="Home_Language__pc" aura:id="iHomeLanguage" required="true"/>
                                    </lightning:layoutItem>
                                    <lightning:layoutItem padding="" size="6" smallDeviceSize="12" mediumDeviceSize="6">
                                        <lightning:inputField disabled="{!v.isFormReadOnly}" fieldName="Communication_Language__pc" aura:id="iCommunicationLanguage" required="true"/>
                                    </lightning:layoutItem>
                                </aura:if>
                                
                                <lightning:layoutItem padding="" size="6" smallDeviceSize="12" mediumDeviceSize="6">
                                    <lightning:inputField disabled="{!v.isFormReadOnly}" fieldName="Country_of_Citizenship__c" aura:id="iNationality" required="true"/>
                                    <div aura:id="errNationality" class="slds-hide" style="color: var(--lwc-colorTextError,rgb(194, 57, 52)) !important;">Please Enter Country of Citizenship</div>
                                </lightning:layoutItem>
                                
                                <!--TdB - Show/Hide fields based on New to Bank/Lite Onboarding -->
                                
                                <aura:if isTrue="{!v.isSoleTrader == false}">
                                    <lightning:layoutItem padding="" size="6" smallDeviceSize="12" mediumDeviceSize="6">
                                        <lightning:inputField disabled="{!v.isFormReadOnly}" fieldName="Country_of_Residence__pc" aura:id="iCountryOfResidence" required="true"/>
                                        <div aura:id="errCountryOfResidence" class="slds-hide" style="color: var(--lwc-colorTextError,rgb(194, 57, 52)) !important;">Please Enter Country Of Residence</div>
                                    </lightning:layoutItem>
                                </aura:if>
                                <!--Haritha - Show/Hide fields based on New to SPM Onboarding -->                                 
                                <aura:if isTrue="{!v.accountRecord.Country_of_Residence__pc != 'South Africa'}">
                                    <lightning:layoutItem padding="" size="6" smallDeviceSize="12" mediumDeviceSize="6">
                                        <lightning:radioGroup name="iTemporaryresident"
                                                              label="Are you a temporary resident of South Africa ?"
                                                              options="{!v.tempresidentdetailOptions }"
                                                              value="{!v.optionGiven}"
                                                              onchange="{!c.showTempresidentDetails}"
                                                              type="button"
                                                              disabled="{!v.isFormReadOnly}" />
                                    </lightning:layoutItem>
                                </aura:if>
                                <aura:if isTrue="{!v.showTempResidentDetails=='Yes'}">
                                    <lightning:layoutItem padding="" size="6" smallDeviceSize="12" mediumDeviceSize="6">
                                        <lightning:inputField disabled="{!v.isFormReadOnly}" fieldName="Temporary_resident_permit_number__c" aura:id="Tempresidentnumber" />
                                    </lightning:layoutItem>
                                    
                                    <lightning:layoutItem padding="" size="6" smallDeviceSize="12" mediumDeviceSize="6">
                                    </lightning:layoutItem>
                                    
                                    <lightning:layoutItem padding="" size="6" smallDeviceSize="12" mediumDeviceSize="6">
                                        <lightning:inputField disabled="{!v.isFormReadOnly}" fieldName="Temporary_Permit_Expiry_Date__c" aura:id="Tempresidentexpirydate"  />
                                    </lightning:layoutItem>
                                    <lightning:layoutItem padding="" size="6" smallDeviceSize="12" mediumDeviceSize="6">
                                    </lightning:layoutItem>
                                    
                                </aura:if>
                                <aura:if isTrue="{!v.showTempResidentDetails=='No'}">
                                    
                                    <lightning:layoutItem padding="" size="6" smallDeviceSize="12" mediumDeviceSize="6">
                                    </lightning:layoutItem>
                                    <lightning:layoutItem padding="" size="6" smallDeviceSize="12" mediumDeviceSize="6">
                                        <lightning:radioGroup name="iAsylumseeker"
                                                              label="Are you a Temporary Resident Seeker ?"
                                                              options="{!v.asylumDetailOptions }"
                                                              value="{!v.asylumoptionGiven}"
                                                              onchange="{!c.showasylumDetailssection}"
                                                              type="button"
                                                              disabled="{!v.isFormReadOnly}" />
                                    </lightning:layoutItem>
                                </aura:if>
                                <aura:if isTrue="{!v.showAsylumDetails=='Yes'}">
                                    <lightning:layoutItem padding="" size="6" smallDeviceSize="12" mediumDeviceSize="6">
                                        <lightning:inputField disabled="{!v.isFormReadOnly}" fieldName="Asylum_Seeker_Permit_Number__c" aura:id="Asylumseekernumber" />
                                    </lightning:layoutItem>
                                    
                                    <lightning:layoutItem padding="" size="6" smallDeviceSize="12" mediumDeviceSize="6">
                                    </lightning:layoutItem>
                                    
                                    <lightning:layoutItem padding="" size="6" smallDeviceSize="12" mediumDeviceSize="6">
                                        <lightning:inputField disabled="{!v.isFormReadOnly}" fieldName="Asylum_Permit_Expiry_Date__c" aura:id="Asylumseekerexpirydate"  />
                                    </lightning:layoutItem>
                                    <lightning:layoutItem padding="" size="6" smallDeviceSize="12" mediumDeviceSize="6">
                                    </lightning:layoutItem>
                                    
                                </aura:if>
                                
                                <!--TdB - Show/Hide fields based on New to Bank/Lite Onboarding -->
                                <aura:if isTrue="{!v.processType != 'Lite Onboarding'}">
                                    <lightning:layoutItem padding="" size="6" smallDeviceSize="12" mediumDeviceSize="6">
                                        <lightning:inputField disabled="{!v.isFormReadOnly}" fieldName="City_of_Residence__c" aura:id="cityOfResidence" required="true"/> <!--Added by Chandra against W-005536-->
                                    </lightning:layoutItem>
                                    <lightning:layoutItem padding="" size="6" smallDeviceSize="12" mediumDeviceSize="6">
                                        <lightning:inputField disabled="{!v.isFormReadOnly}" fieldName="Post_Matric_Qualification__pc" aura:id="iMatricQualification"/>
                                    </lightning:layoutItem>
                                    <lightning:layoutItem padding="" size="6" smallDeviceSize="12" mediumDeviceSize="6">
                                        <lightning:inputField disabled="{!v.isFormReadOnly}" fieldName="Number_Of_Dependents__c" aura:id="iNumberOfDependents" required="true"/> 
                                    </lightning:layoutItem>
                                    <lightning:layoutItem padding="" size="6" smallDeviceSize="12" mediumDeviceSize="6">
                                        <lightning:inputField disabled="{!v.isFormReadOnly}" fieldName="Residential_Status__pc" aura:id="iResidentialStatus" required="true"/>
                                    </lightning:layoutItem>
                                    <lightning:layoutItem padding="" size="6" smallDeviceSize="12" mediumDeviceSize="6">
                                        <lightning:inputField disabled="{!v.isFormReadOnly}" fieldName="Is_Customer_Multi_Banked__c" aura:id="multiBanked" />
                                    </lightning:layoutItem>
                                    <lightning:layoutItem padding="" size="6" smallDeviceSize="12" mediumDeviceSize="6">
                                        <lightning:inputField disabled="{!v.isFormReadOnly}" fieldName="Nationality__pc" aura:id="iNationality" required="true"/>
                                    </lightning:layoutItem>
                                </aura:if>
                                
                                <!--TdB: Hide Sole Trader fields fron Individual W-005524  -->
                                <aura:if isTrue="{!v.isSoleTrader}">
                                    
                                    <!--TdB - Show/Hide fields based on New to Bank/Lite Onboarding -->
                                    <aura:if isTrue="{!v.processType != 'Lite Onboarding'}">
                                        <lightning:layoutItem padding="" size="6" smallDeviceSize="12" mediumDeviceSize="6">
                                            <label for="mangrSoleTrader">Is there a Manager of the Company other than the Sole Trader ?</label>
                                            <lightning:inputField disabled="{!v.isFormReadOnly}" variant="label-hidden" fieldName="Manager_other_than_the_Sole_Trader__c" aura:id="mangrSoleTrader"/>
                                        </lightning:layoutItem>
                                    </aura:if>
                                    
                                    <lightning:layoutItem padding="" size="6" smallDeviceSize="12" mediumDeviceSize="6">
                                        <lightning:inputField disabled="{!v.isFormReadOnly}" fieldName="Countries_Traded_With__c" aura:id="iCountriesTradedWith" required="true"/>
                                    </lightning:layoutItem>
