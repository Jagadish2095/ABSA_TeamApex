                                                          label="{! v.foreignTax.label }"
                                                          value="{! v.foreignTax.value }"
                                                          required="{! v.foreignTax.required }"/>
                                </div>
                            </lightning:layoutitem>
                        </lightning:layout>
                        <aura:if isTrue="{! v.foreignTax.value == 'Yes' }">
                            <lightning:layout>
                                <lightning:layoutitem padding="horizontal-medium" size="12" >
                                    <div id="{! globalId + '_foreignTaxDataTable' }" class="slds-var-p-around_x-small">
                                        <c:ForeignTaxDataTable aura:id="foreignTaxDataTable"
                                                               columns="{!v.foreignTaxColumns}"
                                                               data="{!v.foreignTaxData}"
                                                               deleteData="{!v.foreignTaxDeleteData}"
                                                               recordId="{!v.recordId}"/>
                                    </div>
                                </lightning:layoutitem>
                            </lightning:layout>
                        </aura:if>
