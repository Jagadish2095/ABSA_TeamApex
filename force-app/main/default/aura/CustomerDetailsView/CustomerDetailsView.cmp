<!--
  @description       : Branch Customer Details View (DO NOT EDIT!)
  @author            : clifford.willers@absa.africa
  @last modified on  : 2021-10-20
  @last modified by  : clifford.willers@absa.africa
-->
<aura:component implements="lightning:availableForFlowScreens,force:appHostable,flexipage:availableForAllPageTypes,flexipage:availableForRecordHome,force:hasRecordId"
                extends="c:CustomerDetailsBase"
                access="global"
                controller="BranchCustomerController">
    <!-- aura:component attributes -->
    <aura:attribute name="editButton" type="Map" default="{ label: 'Edit', variant: 'brand', disabled: false }"/>
    <aura:attribute name="processType" type="String" default="" />
    <aura:attribute name="dhaAttestation" type="Boolean" default="false" />
    <aura:attribute name="recordLoaded" type="Boolean" default="false" />
    <aura:attribute name="submitting" type="Boolean" default="false" />
    <aura:attribute name="postalAddressSame" type="String" default="No" />
    <aura:attribute name="readOnly" type="Boolean" default="true" />
    <aura:attribute name="loadedMissingData" type="List" default="[]"/>
    <aura:attribute name="maintenanceCase" type="Object"/>
    <aura:attribute name="caseCreated" type="Boolean" default="false" />
    <aura:attribute name="firstLoad" type="Boolean" default="false" />
    <!-- KYC attributes -->
    <aura:attribute name="kycFieldChanged" type="Boolean" default="false" />
    <!-- <aura:attribute name="kycChanged" type="String" /> -->
    <aura:attribute name="kycFieldList" type="List" default="[]"/>
    <!-- BranchCustomerHub attributes -->
    <aura:attribute name="startNewFlow" type="boolean" default="false" />
    <aura:attribute name="selectedProcess" type="Object" />
    <aura:attribute name="complianceMissingData" type="List" default="[]" />
    <!-- show component section attributes -->
    <aura:attribute name="showPersonalInformation" type="Boolean" default="true" />
    <aura:attribute name="showContactInformation" type="Boolean" default="true" />
    <aura:attribute name="showMarketingConsent" type="Boolean" default="true" />
    <aura:attribute name="showAddressInformation" type="Boolean" default="true" />
    <aura:attribute name="showNextOfKinInformation" type="Boolean" default="true" />
    <aura:attribute name="showEmploymentInformation" type="Boolean" default="true" />
    <aura:attribute name="showEmployersInformation" type="Boolean" default="true" />
    <aura:attribute name="showTaxInformation" type="Boolean" default="true" />
    <!-- lightning:select Option Lists -->
    <aura:attribute name="clientTypeOptions" type="List"/>
    <aura:attribute name="titleOptions" type="List"/>
    <aura:attribute name="maritalStatusOptions" type="List"/>
    <aura:attribute name="maritalContractTypeOptions" type="List"/>
    <aura:attribute name="countryOfBirthOptions" type="List"/>
    <aura:attribute name="nationalityOptions" type="List"/>
    <aura:attribute name="homeLanguageOptions" type="List"/>
    <aura:attribute name="incomeSourceOptions" type="string"/>
    <aura:attribute name="communicationLanguageOptions" type="List"/>
    <aura:attribute name="communicationMethodOptions" type="List"/>
    <aura:attribute name="residentialStatusOptions" type="List"/>
    <aura:attribute name="postalAddressOptions" type="List"/>
    <aura:attribute name="relationshipOptions" type="List" />
    <aura:attribute name="occupationStatusOptions" type="List" />
    <aura:attribute name="occupationLevelOptions" type="List" />
    <aura:attribute name="occupationCategoryOptions" type="List" />
    <aura:attribute name="qualificationOptions" type="List" />
    <aura:attribute name="employerSectorOptions" type="List" />
    <aura:attribute name="hasQualificationOptions" type="List" />
    <aura:attribute name="incomeTaxOptions" type="List" />
    <aura:attribute name="foreignTaxOptions" type="List" />
    <aura:attribute name="incomeTaxReasonOptions" type="List"/>
    <aura:attribute name="nonCreditMarketingOptions" type="List" />
    <aura:attribute name="nonCreditMarketingIndicators" type="List" />
    <aura:attribute name="creditMarketingOptions" type="List" />
    <aura:attribute name="creditMarketingIndicators" type="List" />
    <aura:attribute name="foreignTaxCountryOptions" type="List" />
    <aura:attribute name="foreignTaxReasonOptions" type="List" />
    <!-- Field Attributes: Personal Information -->
    <aura:attribute name="clientType" type="Map" default="{ label: 'Client type', required: true, disabled: false }"/>
    <aura:attribute name="titleMap" type="Map" default="{ label: 'Title', required: true, disabled: false }"/>
    <aura:attribute name="firstNameMap" type="Map" default="{ label: 'Full names', required: true, disabled: false }"/>
    <aura:attribute name="lastNameMap" type="Map" default="{ label: 'Surname', required: true, disabled: false }"/>
    <aura:attribute name="iDNumberMap" type="Map" default="{ label: 'ID number', required: true, disabled: true }"/>
    <aura:attribute name="dateIssuedMap" type="Map" default="{ label: 'Date issued', required: true, disabled: false }"/>
    <aura:attribute name="incomeSourceMap" type="Map" default="{ label: 'Source of income', required: true, disabled: false }"/>
    <aura:attribute name="maritalStatusMap" type="Map" default="{ label: 'Marital status', required: true, disabled: false }"/>
    <aura:attribute name="maritalTypeMap" type="Map" default="{ label: 'Marriage type', required: true, disabled: false }"/>
    <aura:attribute name="countryOfBirthMap" type="Map" default="{ label: 'Country of birth', required: true, disabled: false }"/>
    <aura:attribute name="homeLanguageMap" type="Map" default="{ label: 'Home language', required: true, disabled: false }"/>
    <aura:attribute name="nationalityMap" type="Map" default="{ label: 'Nationality', required: true, disabled: false }"/>
    <aura:attribute name="homeLanguageOtherMap" type="Map" default="{ label: 'Please specify', required: true, disabled: false }"/>
    <!-- Field Attributes: Contact Information -->
    <aura:attribute name="personEmail" type="Map" default="{ label: 'Email address', required: false, disabled: false }"/>
    <aura:attribute name="personOtherPhone" type="Map" default="{ label: 'Alternative contact number (optional)', required: false, disabled: false }"/>
    <aura:attribute name="communicationLanguage" type="Map" default="{ label: 'Communication language', required: true, disabled: false }"/>
    <aura:attribute name="personMobilePhone" type="Map" default="{ label: 'Cellphone number', required: true, disabled: false }"/>
    <aura:attribute name="communicationMethod" type="Map" default="{ label: 'Preferred communication channel', required: true, disabled: false }"/>
    <!-- Field Attributes: Marketing Consent   -->
    <aura:attribute name="nonCreditMarketing" type="Map" default="{ value: '', label: 'Be the first to hear about exciting new products, upgrade opportunities, fantastic offers and even more Rewards from Absa Group. Interested?', required: true, disabled: false }"/>
    <aura:attribute name="nonCreditCheck" type="Map" default="{ value: '', label: 'Are you sure? You can choose just how you hear about our offers' }"/>
    <aura:attribute name="creditMarketing" type="Map" default="{ value: '', label: 'You may qualify for future loans and limit increases from Absa Group. Would you like to hear about these?', required: true, disabled: false }"/>
    <aura:attribute name="creditCheck" type="Map" default="{ value: '', label: 'Please contact me via:' }"/>
    <!-- Field Attributes: Address information -->
    <aura:attribute name="residentialStatus" type="Map" default="{ label: 'Residential status', required: true, disabled: false }"/>
    <aura:attribute name="residentialAddress" type="Map" default="{ loaded: false, type: 'Residential', street: '', street2: '', suburb: '', city: '', province: '', code: '', country: '', required: true, disabled: false, readOnly: false }"/>
    <aura:attribute name="residentialAddressKyc" type="Map" default="{ street: '', street2: '', suburb: '', city: '', province: '', code: '', country: '' }"/>
    <aura:attribute name="postalAddress" type="Map" default="{ loaded: false, type: 'Postal', street: '', street2: '', suburb: '', city: '', province: '', code: '', country: '', required: true, disabled: false, readOnly: false }"/>
    <!-- Field Attributes: Next of Kin -->
    <aura:attribute name="kinFirstNameMap" type="Map" default="{ label: 'First name', required: true, disabled: false }"/>
    <aura:attribute name="kinLastNameMap" type="Map" default="{ label: 'Surname', required: true, disabled: false }"/>
    <aura:attribute name="kinRelationshipMap" type="Map" default="{ label: 'Relationship', required: true, disabled: false }"/>
    <aura:attribute name="kinCellphoneNumberMap" type="Map" default="{ label: 'Cellphone number', required: true, disabled: false }"/>
    <aura:attribute name="kinTelephoneNumberMap" type="Map" default="{ label: 'Work number (optional)', required: false, disabled: false }"/>
    <aura:attribute name="kinEmailAddressMap" type="Map" default="{ label: 'Email address (optional)', required: false, disabled: false }"/>
    <!-- Field Attributes: Employment details -->
    <aura:attribute name="occupationStatus" type="Map" default="{ value: '', label: 'Occupation status', required: true, disabled: false }"/>
    <aura:attribute name="occupationLevel" type="Map" default="{ value: '', label: 'Occupation level', required: true, disabled: false }"/>
    <aura:attribute name="occupationCategory" type="Map" default="{ value: '', label: 'Occupation category', required: true, disabled: false }"/>
    <aura:attribute name="employerSector" type="Map" default="{ value: '', label: 'Industry', required: true, disabled: false }"/>
    <aura:attribute name="hasQualification" type="Map" default="{ value: '', label: 'Does the customer have a post matric qualification?', required: true }"/>
    <aura:attribute name="qualification" type="Map" default="{ value: '', label: 'Post matric qualification', required: true, disabled: false }"/>
    <!-- Field Attributes: Employers details -->
    <aura:attribute name="employerName" type="Map" default="{ value: '', label: 'Employers name', required: true, disabled: false }"/>
    <aura:attribute name="employerPhone" type="Map" default="{ value: '', label: 'Employers phone number', required: true, disabled: false }"/>
    <aura:attribute name="employersAddress" type="Map" default="{ loaded: false, type: 'Employers', street: '', street2: '', suburb: '', city: '', province: '', code: '', country: '', required: true, disabled: false }"/>
    <!-- Field Attributes: Tax details -->
    <aura:attribute name="incomeTax" type="Map" default="{ value: '', label: 'Is the customer registered for income tax?', required: true, disabled: false }"/>
    <aura:attribute name="foreignTax" type="Map" default="{ value: '', label: 'Is the customer registered for foreign income tax?', required: true, disabled: false }"/>
    <aura:attribute name="incomeTaxReason" type="Map" default="{ value: '', label: 'Reason SA income tax number not given', required: true, disabled: false }"/>
    <aura:attribute name="incomeTaxNumber" type="Map" default="{ value: '', label: 'SA income tax number (optional)', required: false, disabled: false }"/>
    <!-- foreignTax: dataTable -->
    <aura:attribute name="foreignTaxData" type="Object" />
    <aura:attribute name="foreignTaxDeleteData" type="Object" />
    <aura:attribute name="foreignTaxCVSData" type="String" />
    <aura:attribute name="foreignTaxColumns" type="List" />
    <aura:attribute name="taxReadOnlyColumns" type="List" />
    <!-- aura:handlers -->
    <aura:handler name="init" value="{! this }" action="{! c.init }" />
    <aura:handler name="change" value="{! v.maritalStatusMap.value }" action="{! c.maritalStatusChanged }" />
    <aura:handler name="change" value="{! v.occupationStatus.value }" action="{! c.occupationStatusChange }" />
    <aura:handler name="change" value="{! v.communicationMethod.value }" action="{! c.checkForEmail }" />
    <aura:handler name="change" value="{! v.residentialAddress.street }" action="{! c.addressChanged }"/>
    <aura:handler name="change" value="{! v.residentialAddress.street2 }" action="{! c.addressChanged }" />
    <aura:handler name="change" value="{! v.residentialAddress.suburb }" action="{! c.addressChanged }" />
    <aura:handler name="change" value="{! v.residentialAddress.city }" action="{! c.addressChanged }" />
    <aura:handler name="change" value="{! v.residentialAddress.province }" action="{! c.addressChanged }" />
    <aura:handler name="change" value="{! v.residentialAddress.code }" action="{! c.addressChanged }" />
    <aura:handler name="change" value="{! v.residentialAddress.country }" action="{! c.addressChanged }" />
    <aura:handler name="change" value="{! v.complianceMissingData }" action="{! c.checkMissingData }" />
    <aura:handler name="change" value="{! v.foreignTaxCountryOptions }" action="{! c.foreignTaxOptionsChanged }" />
    <aura:handler name="change" value="{! v.foreignTaxReasonOptions }" action="{! c.foreignTaxOptionsChanged }" />
    <aura:handler name="change" value="{!v.dialogSelection}" action="{!c.dialogSelectionChanged}" />
    <aura:handler event="aura:doneRendering" action="{!c.checkMissingData}"/>
    <aura:handler name="addressRecordEvent" event="c:BranchAddressRecordEvent" action="{! c.handleRecordEvent }"/>
    <aura:handler action="{!c.handleNavigate}" event="c:BranchNavigateFlow"/>

    <lightning:recordEditForm aura:id = "CustomerInformation"
                              recordId="{! v.recordId }"
                              objectApiName="Account"
                              density="comfy"
                              onload="{! c.recordLoaded }"
                              onsubmit="{! c.recordSubmit }"
                              onerror="{! c.recordError }"
                              onsuccess="{! c.recordSuccess }">
        <lightning:inputField fieldName="Client_Type__c" class="hidden_field"/>
        <lightning:inputField fieldName="FirstName" class="hidden_field"/>
        <lightning:inputField fieldName="Titles__pc" class="hidden_field"/>
        <lightning:inputField fieldName="ID_Number__pc" class="hidden_field"/>
        <lightning:inputField fieldName="LastName" class="hidden_field"/>
        <lightning:inputField fieldName="Date_Issued__pc" class="hidden_field"/>
        <lightning:inputField fieldName="Marital_Status__pc" class="hidden_field"/>
        <lightning:inputField fieldName="Country_of_Birth__pc" class="hidden_field"/>
        <lightning:inputField fieldName="Home_Language__pc" class="hidden_field"/>
        <lightning:inputField fieldName="Home_Language_Other__pc" class="hidden_field"/>
        <lightning:inputField fieldName="Marital_Contract_Type__pc" class="hidden_field"/>
        <lightning:inputField fieldName="Nationality_List__pc" class="hidden_field"/>
        <lightning:inputField fieldName="Income_Source__pc" class="hidden_field"/>

        <lightning:inputField fieldName="PersonEmail" class="hidden_field"/>
        <lightning:inputField fieldName="PersonOtherPhone" class="hidden_field"/>
        <lightning:inputField fieldName="Communication_Language__pc" class="hidden_field"/>
        <lightning:inputField fieldName="PersonMobilePhone" class="hidden_field"/>
        <lightning:inputField fieldName="Preferred_Communication_Method__pc" class="hidden_field"/>
        <lightning:inputField fieldName="Residential_Status__pc" class="hidden_field"/>

        <lightning:inputField fieldName="Next_of_Kin_First_Name_s__pc" class="hidden_field"/>
        <lightning:inputField fieldName="Next_of_Kin_Last_Name__pc" class="hidden_field"/>
        <lightning:inputField fieldName="Next_of_Kin_Relationship__pc" class="hidden_field"/>
        <lightning:inputField fieldName="Next_of_Kin_Cellphone_Number__pc" class="hidden_field"/>
        <lightning:inputField fieldName="Next_of_Kin_Telephone_Number__pc" class="hidden_field"/>
        <lightning:inputField fieldName="Next_of_Kin_Email_Address__pc" class="hidden_field"/>

        <lightning:inputField fieldName="Occupation_Status__pc" class="hidden_field"/>
        <lightning:inputField fieldName="Occupation_Level__pc" class="hidden_field"/>
        <lightning:inputField fieldName="Occupation_Category__pc" class="hidden_field"/>
        <lightning:inputField fieldName="Has_Post_Matric_Qualification__pc" class="hidden_field"/>
        <lightning:inputField fieldName="Post_Matric_Qualification__pc" class="hidden_field"/>
        <lightning:inputField fieldName="Employer_Sector__pc" class="hidden_field"/>
        <lightning:inputField fieldName="Employer_Name__pc" class="hidden_field"/>
        <lightning:inputField fieldName="Employer_Phone__c" class="hidden_field"/>

        <lightning:inputField fieldName="Is_customer_registered_for_income_tax__c" class="hidden_field"/>
        <lightning:inputField fieldName="Is_customer_registered_for_foreign_tax__c" class="hidden_field"/>
        <lightning:inputField fieldName="Income_Tax_Number__pc" class="hidden_field"/>
        <lightning:inputField fieldName="Income_Tax_Number_Reason__pc" class="hidden_field"/>

        <lightning:inputField fieldName="Non_Credit_Marketing_Consent__pc" class="hidden_field"/>
        <lightning:inputField fieldName="Non_Credit_Marketing_Indicator__pc" class="hidden_field"/>
        <lightning:inputField fieldName="Credit_Marketing_Consent__pc" class="hidden_field"/>
        <lightning:inputField fieldName="Credit_Marketing_Indicator__pc" class="hidden_field"/>

        <button id="{! globalId + '_information_submit' }" type="submit" class="hidden_field"/>
    </lightning:recordEditForm>

    <aura:if isTrue="{! v.readOnly }">
        <lightning:card>
            <aura:if isTrue="{! v.showPersonalInformation }">
                <lightning:card>
                    <aura:set attribute="title">
                        <b>Personal Information</b>
                    </aura:set>
                    <aura:set attribute="actions">
                        <lightning:button aura:id="Edit"
                                          label="{! v.editButton.label }"
                                          variant="{! v.editButton.variant }"
                                          disabled="{! v.editButton.disabled }"
                                          iconName="utility:edit"
                                          iconPosition="left"
                                          onclick="{! c.onEditButtonPressed }" />
                    </aura:set>
                    <lightning:layout>
                        <lightning:layoutitem padding="horizontal-medium" size="6" >
                            <div class="slds-var-p-around_x-small">
                                <label id="{! globalId + '_titleReadOnly_label' }" for="titleReadOnly" >{! v.titleMap.label }</label>
                                <div id="{! globalId + '_titleReadOnly' }" class="slds-border_bottom slds-var-p-top_xx-small slds-var-p-bottom_x-small">
                                    <b><lightning:formattedText aura:id="titleReadOnly"
                                                                name="titleReadOnly"
                                                                value="{! v.titleMap.value }"/></b>
                                </div>
                            </div>
                        </lightning:layoutitem>
                        <lightning:layoutitem padding="horizontal-medium" size="6" >
                            <div class="slds-var-p-around_x-small">
                                <label id="{! globalId + '_ClientTypeReadOnly_label' }" for="ClientTypeReadOnly" >{! v.clientType.label }</label>
                                <div id="{! globalId + '_ClientTypeReadOnly' }" class="slds-border_bottom slds-var-p-top_xx-small slds-var-p-bottom_x-small">
                                    <b><lightning:formattedText aura:id="ClientTypeReadOnly"
                                                                name="ClientTypeReadOnly"
                                                                value="{! v.clientType.value }"/></b>
                                </div>
                            </div>
                        </lightning:layoutitem>
                    </lightning:layout>
                    <lightning:layout>
                        <lightning:layoutitem padding="horizontal-medium" size="6">
                            <div class="slds-var-p-around_x-small">
                                <label id="{! globalId + '_fullNameReadOnly_label' }" for="fullNameReadOnly" >{! v.firstNameMap.label }</label>
                                <div id="{! globalId + '_fullNameReadOnly' }" class="slds-border_bottom slds-var-p-top_xx-small slds-var-p-bottom_x-small">
                                    <b><lightning:formattedText aura:id="fullNameReadOnly"
                                                                name="fullNameReadOnly"
                                                                value="{! v.firstNameMap.value }"/></b>
                                </div>
                            </div>
                            <div class="slds-var-p-around_x-small">
                                <label id="{! globalId + '_IdNumberReadOnly_label' }" for="IdNumberReadOnly" >{! v.iDNumberMap.label }</label>
                                <div id="{! globalId + '_IdNumberReadOnly' }" class="slds-border_bottom slds-var-p-top_xx-small slds-var-p-bottom_x-small">
                                    <b><lightning:formattedText aura:id="IdNumberReadOnly"
                                                                name="IdNumberReadOnly"
                                                                value="{! v.iDNumberMap.value }"/></b>
                                </div>
                            </div>
                        </lightning:layoutitem>
                        <lightning:layoutitem padding="horizontal-medium" size="6">
                            <div class="slds-var-p-around_x-small">
                                <label id="{! globalId + '_LastNameReadOnly_label' }" for="LastNameReadOnly" >{! v.lastNameMap.label }</label>
                                <div id="{! globalId + '_LastNameReadOnly' }" class="slds-border_bottom slds-var-p-top_xx-small slds-var-p-bottom_x-small">
                                    <b><lightning:formattedText aura:id="LastNameReadOnly"
                                                                name="LastNameReadOnly"
                                                                value="{! v.lastNameMap.value }"/></b>
                                </div>
                            </div>
                            <div class="slds-var-p-around_x-small">
                                <label id="{! globalId + '_DateIdIssuedReadOnly_label' }" for="DateIdIssuedReadOnly" >{! v.dateIssuedMap.label }</label>
                                <div id="{! globalId + '_DateIdIssuedReadOnly' }" class="slds-border_bottom slds-var-p-top_xx-small slds-var-p-bottom_x-small">
                                    <b><lightning:formattedDateTime aura:id="DateIdIssuedReadOnly"
                                                                    name="DateIdIssuedReadOnly"
                                                                    value="{! v.dateIssuedMap.value }"/></b>
                                </div>
                            </div>
                        </lightning:layoutitem>
                    </lightning:layout>
                    <lightning:layout>
                        <lightning:layoutitem padding="horizontal-medium" size="6" >
                            <div class="slds-var-p-around_x-small">
                                <label id="{! globalId + '_SourceOfIncomeReadOnly_label' }" for="SourceOfIncomeReadOnly" >{! v.incomeSourceMap.label }</label>
                                <div id="{! globalId + '_SourceOfIncomeReadOnly' }" class="slds-border_bottom slds-var-p-top_xx-small slds-var-p-bottom_x-small">
                                    <b><lightning:formattedText aura:id="SourceOfIncomeReadOnly"
                                                                name="SourceOfIncomeReadOnly"
                                                                value="{! v.incomeSourceMap.value }"/></b>
                                </div>
                            </div>
                        </lightning:layoutitem>
                    </lightning:layout>
                    <lightning:layout>
                        <lightning:layoutitem padding="horizontal-medium" size="6" >
                            <div class="slds-var-p-around_x-small">
                                <label id="{! globalId + '_MaritalStatusReadOnly_label' }" for="MaritalStatusReadOnly" >{! v.maritalStatusMap.label }</label>
                                <div id="{! globalId + '_MaritalStatusReadOnly' }" class="slds-border_bottom slds-var-p-top_xx-small slds-var-p-bottom_x-small">
                                    <b><lightning:formattedText aura:id="MaritalStatusReadOnly"
                                                                name="MaritalStatusReadOnly"
                                                                value="{! v.maritalStatusMap.value }"/></b>
                                </div>
                            </div>
                        </lightning:layoutitem>
                        <lightning:layoutitem padding="horizontal-medium" size="6" >
                            <aura:if isTrue="{! v.maritalStatusMap.value == 'Married' }">
                                <div class="slds-var-p-around_x-small">
                                    <label id="{! globalId + '_MaritalContractTypeReadOnly_label' }" for="MaritalContractTypeReadOnly" >{! v.maritalTypeMap.label }</label>
                                    <div id="{! globalId + '_MaritalContractTypeReadOnly' }" class="slds-border_bottom slds-var-p-top_xx-small slds-var-p-bottom_x-small">
                                        <b><lightning:formattedText aura:id="MaritalContractTypeReadOnly"
                                                                    name="MaritalContractTypeReadOnly"
                                                                    value="{! v.maritalTypeMap.value }"/></b>
                                    </div>
                                </div>
                            </aura:if>
                        </lightning:layoutitem>
                    </lightning:layout>
                    <lightning:layout>
                        <lightning:layoutitem padding="horizontal-medium" size="6">
                            <div class="slds-var-p-around_x-small">
                                <label id="{! globalId + '_countryOfBirthReadOnly_label' }" for="countryOfBirthReadOnly" >{! v.countryOfBirthMap.label }</label>
                                <div id="{! globalId + '_countryOfBirthReadOnly' }" class="slds-border_bottom slds-var-p-top_xx-small slds-var-p-bottom_x-small">
                                    <b><lightning:formattedText aura:id="countryOfBirthReadOnly"
                                                                name="countryOfBirthReadOnly"
                                                                value="{! v.countryOfBirthMap.value }"/></b>
                                </div>
                            </div>
                        </lightning:layoutitem>
                        <lightning:layoutitem padding="horizontal-medium" size="6">
                            <div class="slds-var-p-around_x-small">
                                <label id="{! globalId + '_nationalityReadOnly_label' }" for="nationalityReadOnly" >{! v.nationalityMap.label }</label>
                                <div id="{! globalId + '_nationalityReadOnly' }" class="slds-border_bottom slds-var-p-top_xx-small slds-var-p-bottom_x-small">
                                    <b><lightning:formattedText aura:id="nationalityReadOnly"
                                                                name="nationalityReadOnly"
                                                                value="{! v.nationalityMap.value }"/></b>
                                </div>
                            </div>
                        </lightning:layoutitem>
                    </lightning:layout>
                    <lightning:layout>
                        <lightning:layoutitem padding="horizontal-medium" size="6">
                            <div class="slds-var-p-around_x-small">
                                <label id="{! globalId + '_HomeLanguageReadOnly_label' }" for="HomeLanguageReadOnly" >{! v.homeLanguageMap.label }</label>
                                <div id="{! globalId + '_HomeLanguageReadOnly' }" class="slds-border_bottom slds-var-p-top_xx-small slds-var-p-bottom_x-small">
                                    <b><lightning:formattedText aura:id="HomeLanguageReadOnly"
                                                                name="HomeLanguageReadOnly"
                                                                value="{! v.homeLanguageMap.value }"/></b>
                                </div>
                            </div>
                        </lightning:layoutitem>
                        <lightning:layoutitem padding="horizontal-medium" size="6">
                            <aura:if isTrue="{! v.homeLanguageMap.value == 'Other' }">
                                <div class="slds-var-p-around_x-small">
