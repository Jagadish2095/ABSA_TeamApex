                        <aura:if isTrue="{! v.incomeTax.value == 'Yes' }">
                            <lightning:layout>
                                <lightning:layoutitem padding="horizontal-medium" size="6" >
                                        <div id="{! globalId + '_IncomeTaxNumber' }" class="slds-var-p-around_x-small">
