                                <div id="{! globalId + '_OccupationCategory' }" class="slds-var-p-around_x-small">
                                    <label for="OccupationCategory">{! v.occupationCategory.label }</label>
                                    <lightning:select aura:id="OccupationCategory"
                                                      name="OccupationCategory"
                                                      label=""
                                                      class="label-hidden"
                                                      value="{! v.occupationCategory.value }"
                                                      required="{! v.occupationCategory.required }"
                                                      disabled="{! v.occupationCategory.disabled }">
                                        <option value="">please select...</option>
                                        <aura:iteration items="{! v.occupationCategoryOptions }" var="oCat">
                                            <option value="{!oCat}" selected="{!oCat==v.occupationCategory.value }">{!oCat}</option>
                                        </aura:iteration>
                                    </lightning:select>
                                </div>
                                <div id="{! globalId + '_Industry' }" class="slds-var-p-around_x-small">
                                    <label for="Industry">{! v.employerSector.label }</label>
                                    <lightning:select aura:id="Industry"
                                                      name="Industry"
                                                      label=""
                                                      class="label-hidden"
                                                      value="{! v.employerSector.value }"
                                                      required="{! v.employerSector.required }"
                                                      disabled="{! v.employerSector.disabled }">
                                        <option value="">please select...</option>
                                        <aura:iteration items="{! v.employerSectorOptions }" var="e">
                                            <option value="{!e}" selected="{!e==v.employerSector.value }">{!e}</option>
                                        </aura:iteration>
                                    </lightning:select>
                                </div>
                            </lightning:layoutitem>
                        </lightning:layout>
                        <lightning:layout>
                            <lightning:layoutitem padding="horizontal-medium" size="6" >
                                <div id="{! globalId + '_HasPostMatricQualificationRadioGroup' }" class="slds-var-p-around_x-small" >
                                    <lightning:radioGroup aura:id="HasPostMatricQualificationRadioGroup"
                                                        type="radio"
                                                        name="HasPostMatricQualificationRadioGroup"
                                                        label="{! v.hasQualification.label }"
                                                        options="{! v.hasQualificationOptions }"
                                                        value="{! v.hasQualification.value }"
                                                        required="{! v.hasQualification.required }"
                                                        onchange="{! c.hasQualificationChange }"/>
                                </div>
                                <aura:if isTrue="{! v.hasQualification.value == 'Yes' }">
                                    <div id="{! globalId + '_PostMatricQualification' }" class="slds-var-p-around_x-small">
                                        <label for="PostMatricQualification" >{! v.qualification.label }</label>
                                        <lightning:select aura:id="PostMatricQualification"
                                                        name="PostMatricQualification"
                                                        label=""
                                                        class="label-hidden"
                                                        value="{! v.qualification.value }"
                                                        required="{! v.qualification.required }"
                                                        disabled="{! v.qualification.disabled }">
                                            <option value="">please select...</option>
                                            <aura:iteration items="{! v.qualificationOptions }" var="q">
                                                <option value="{!q}" selected="{!q==v.qualification.value}">{!q}</option>
                                            </aura:iteration>
                                        </lightning:select>
                                    </div>
                                </aura:if>
                            </lightning:layoutitem>
                        </lightning:layout>
                    </lightning:card>
                </aura:if>
                <aura:if isTrue="{! v.showEmployersInformation }">
