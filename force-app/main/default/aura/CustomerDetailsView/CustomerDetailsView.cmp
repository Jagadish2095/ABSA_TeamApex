                                                        name="NextofKinEmailAddress"
                                                        value="{! v.kinEmailAddressMap.value }"
                                                        variant="label-hidden"
                                                        required="{! v.kinEmailAddressMap.required }"
                                                        disabled="{! v.kinEmailAddressMap.disabled }"
                                                        type="email" />
                                </div>
                            </lightning:layoutitem>
                        </lightning:layout>
                    </lightning:card>
                </aura:if>
                <aura:if isTrue="{! v.showEmploymentInformation }">
                    <lightning:card>
                        <aura:set attribute="title">
                            <b>Employment Details</b>
                        </aura:set>
                        <lightning:layout>
                            <lightning:layoutitem padding="horizontal-medium" size="6">
                                <div id="{! globalId + '_OccupationStatus' }" class="slds-var-p-around_x-small">
                                    <label for="OccupationStatus">{! v.occupationStatus.label }</label>
                                    <lightning:select aura:id="OccupationStatus"
                                                      name="OccupationStatus"
                                                      label=""
                                                      class="label-hidden"
                                                      value="{! v.occupationStatus.value }"
                                                      required="{! v.occupationStatus.required }"
                                                      disabled="{! v.occupationStatus.disabled }">
                                        <option value="">please select...</option>
                                        <aura:iteration items="{! v.occupationStatusOptions }" var="oStatus">
                                            <option value="{!oStatus}" selected="{!oStatus==v.occupationStatus.value}">{!oStatus}</option>
                                        </aura:iteration>
                                    </lightning:select>
                                </div>
                                <div id="{! globalId + '_OccupationLevel' }" class="slds-var-p-around_x-small">
                                    <label for="OccupationLevel">{! v.occupationLevel.label }</label>
                                    <lightning:select aura:id="OccupationLevel"
                                                      name="OccupationLevel"
                                                      label=""
                                                      class="label-hidden"
                                                      value="{! v.occupationLevel.value }"
                                                      required="{! v.occupationLevel.required }"
                                                      disabled="{! v.occupationLevel.disabled }">
                                        <option value="">please select...</option>
                                        <aura:iteration items="{! v.occupationLevelOptions }" var="oLevel">
                                            <option value="{!oLevel}" selected="{!oLevel==v.occupationLevel.value }">{!oLevel}</option>
                                        </aura:iteration>
                                    </lightning:select>
                                </div>
                            </lightning:layoutitem>
