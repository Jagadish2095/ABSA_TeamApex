                                                      addressDisabled="{! v.employersAddress.disabled }"
                                                      addressStreet="{! v.employersAddress.street }"
                                                      addressStreet2="{! v.employersAddress.street2 }"
                                                      addressSuburb="{! v.employersAddress.suburb }"
                                                      addressCity="{! v.employersAddress.city }"
                                                      addressPostalCode="{! v.employersAddress.code }"
                                                      addressProvince="{! v.employersAddress.province }"
                                                      addressCountry="{! v.employersAddress.country }"/>
                            </lightning:layoutItem>
                        </lightning:layout>
                    </lightning:card>
                </aura:if>
                <aura:if isTrue="{! v.showTaxInformation }">
                    <lightning:card>
                        <aura:set attribute="title">
                            <b>Tax Details</b>
                        </aura:set>
                        <lightning:layout>
                            <lightning:layoutitem padding="horizontal-medium" size="12" >
                                <div id="{! globalId + '_IncomeTaxRadioGroup' }" class="slds-var-p-around_x-small" >
