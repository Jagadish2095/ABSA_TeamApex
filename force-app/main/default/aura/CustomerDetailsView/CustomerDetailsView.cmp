                                </lightning:layoutitem>
                            </lightning:layout>
                            <lightning:layout>
                                <lightning:layoutitem padding="horizontal-medium" size="12" >
                                        <div id="{! globalId + '_ReasonSaTaxNotGiven' }" class="slds-var-p-around_x-small">
                                            <label for="ReasonSaTaxNotGiven">{! v.incomeTaxReason.label }</label>
                                            <lightning:select aura:id="ReasonSaTaxNotGiven"
                                                              name="ReasonSaTaxNotGiven"
                                                              label=""
