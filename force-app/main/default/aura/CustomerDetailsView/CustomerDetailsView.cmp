                                                        name="NextofKinLastName"
                                                        pattern="[a-zA-Z\\ \\'\\-]+"
                                                        value="{! v.kinLastNameMap.value }"
                                                        variant="label-hidden"
                                                        required="{! v.kinLastNameMap.required }"
                                                        disabled="{! v.kinLastNameMap.disabled }"/>
                                </div>
                            </lightning:layoutitem>
                        </lightning:layout>
                        <lightning:layout>
                            <lightning:layoutitem padding="horizontal-medium" size="6">
                                <div id="{! globalId + '_NextofKinCellphoneNumber' }" class="slds-var-p-around_x-small">
                                    <label for="NextofKinCellphoneNumber">{! v.kinCellphoneNumberMap.label }</label>
                                    <lightning:input aura:id="NextofKinCellphoneNumber"
                                                        name="NextofKinCellphoneNumber"
                                                        value="{! v.kinCellphoneNumberMap.value }"
                                                        variant="label-hidden"
                                                        required="{! v.kinCellphoneNumberMap.required }"
                                                        disabled="{! v.kinCellphoneNumberMap.disabled }"
                                                        pattern="[0-9]+" />
                                </div>
                                <div id="{! globalId + '_NextofKinTelephoneNumber' }" class="slds-var-p-around_x-small">
                                    <label for="NextofKinTelephoneNumber">{! v.kinTelephoneNumberMap.label }</label>
                                    <lightning:input aura:id="NextofKinTelephoneNumber"
                                                        name="NextofKinTelephoneNumber"
                                                        value="{! v.kinTelephoneNumberMap.value }"
                                                        variant="label-hidden"
                                                        required="{! v.kinTelephoneNumberMap.required }"
                                                        disabled="{! v.kinTelephoneNumberMap.disabled }"
                                                        pattern="[0-9]+" />
                                </div>
                            </lightning:layoutitem>
                            <lightning:layoutitem padding="horizontal-medium" size="6">
                                <div id="{! globalId + '_NextofKinEmailAddress' }" class="slds-var-p-around_x-small">
