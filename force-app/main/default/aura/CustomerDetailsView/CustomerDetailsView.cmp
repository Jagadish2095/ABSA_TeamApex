                                                              value="{! v.incomeTaxReason.value }">
                                                <option value="">please select...</option>
                                                <aura:iteration items="{! v.incomeTaxReasonOptions }" var="taxReason">
                                                    <option value="{!taxReason}" selected="{!taxReason==v.incomeTaxReason.value}">{!taxReason}</option>
                                                </aura:iteration>
                                            </lightning:select>
                                        </div>
                                </lightning:layoutitem>
