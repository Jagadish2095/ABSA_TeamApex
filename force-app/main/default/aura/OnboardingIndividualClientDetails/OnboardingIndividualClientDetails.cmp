<!--
* Lightning Component for Onboarding Client Information (OnboardingIndividualClientDetails)
*
* @author  Manoj Danthala : Absa
* @version v1.0
* @since   2019-11-20
*
-->
<!--
* Prashant Jain and Tinashe Shoko: 20200312: Changes for Merchant Onboarding
-->

<aura:component
	access="global"
	controller="OnboardingIndividualDetailsController"
	implements="lightning:availableForFlowScreens,force:appHostable,flexipage:availableForAllPageTypes,flexipage:availableForRecordHome,force:hasRecordId,forceCommunity:availableForAllPageTypes,force:lightningQuickAction"
>
    <!--Handlers-->
    <aura:handler action="{!c.doInit}" name="init" value="{!this}" />
    <lightning:workspaceAPI aura:id="workspace"/>
    <!--Attributes-->
	<aura:attribute name="occupationLevelList" type="List" />
	<!--Yongama W-006515-->
    <aura:attribute name="employerSectorList" type="List"/>
    <aura:attribute type="List" name="docList" default=""/>
    <aura:attribute name="data" type="Object"/>
    <aura:attribute name="columns" type="List"/> 
    <aura:attribute name="selectedCommMethod" type="String" />
    <aura:attribute name="activeSections" type="List" default="['Consent','Golden Source Documents']" />
	<aura:attribute
		name="defaultActiveSections"
		type="List"
		default="['Consent','Golden Source Documents','Account','Occupation','IDV','NextOfKin','commmunication','BusinessAnalysisDetails','Signatories','BusinessInfo','BusinessAdd','Financial','Relationships','Regulatory', 'tradingAsName', '3IF','fatca','riskIndicator', 'sicCodeClassification']"
	/>
	<!-- PJAIN: 20200319 -->
    <aura:attribute name="accRecordId" type="String" />
    <aura:attribute name="indvRecordTypeId" type="String" />
    <aura:attribute name="accRecordTypeId" type="String" />
    <aura:attribute name="clientGroupValue" type="String" />
    <aura:attribute name="clientTypeValue" type="String" />
    <aura:attribute name="isForeignNational" type="boolean"/>
    <aura:attribute name="isMinorIndiv" type="boolean"/>
    <aura:attribute name="placeofResValue" type="String"/>
    <aura:attribute name="caseRecordId" type="String" />
    <aura:attribute name="record" type="Object"/>
    <aura:attribute name="accountRecord" type="Account" default="{sobjectType:'Account'}"/>
    <aura:attribute name="recordError" type="String"/>
    <aura:attribute name="isConfirmationModalOpen" type="boolean" default="false"/>
	<aura:Attribute name="showRecordEditForm" type="boolean" default="false" />
	<!-- PJAIN: 20200319 -->
    <aura:attribute name="hanisResponse" type="HanisValidate_Resp_IB_v1"/>
    <aura:attribute name="CPBResponse" type="CPBValidate_Resp_IB_v1"/>
    <aura:attribute name="residentialAddressDetails" type="object"/>
    <aura:attribute name="postalAddressDetails" type="object"/>
    <aura:Attribute name="disableNextBtn" type="boolean" default="true"/>
    <aura:attribute name="companyDetails" type="object"/>
    <aura:attribute name="registrationNumber" type="String"/>
    <aura:attribute name="IdentityNumber" type="String"/>
    <aura:Attribute name="errorMessage" type="String" />
    <aura:Attribute name="isshowError" type="boolean" />
    <aura:Attribute name="isSoleTrader" type="boolean" default="false"/>
    <aura:Attribute name="isEmployed" type="boolean" default="false"/>
    <aura:Attribute name="isOtherEmployer" type="boolean" default="false"/><!--Yongama W-006517-->
     <aura:Attribute name="isOtherLevel" type="boolean" default="false"/><!--Yongama W-006516-->
	<aura:Attribute name="OtherOccupation" type="boolean" default="false" />
	<!--Yongama W-006515-->
	<aura:attribute name="ProcessName" type="String" default="" />
	<!-- 20200312: Prashant Jain and Tinashe Shoko: Added for Merchant Onboarding -->
	<aura:attribute name="processType" type="String" default="" />
	<!-- TdB: Defines the Process Type (New to Bank/Lite Onboarding) -->
    <aura:attribute name="highIndustryValue" type="String"/>
    <aura:attribute type="Boolean" name="showNatureOfBusinessModal" default="false" />
    <aura:Attribute name="sicCategoryValue" type="String" />
    <aura:Attribute name="countryofresValue" type="String" />
    <aura:Attribute name="goldenSourceConsentGiven" type="boolean" default="true"/>
    <aura:Attribute name="disableConsentCheckbox" type="boolean" default="false"/>
    <aura:attribute type="Boolean" name="showNoTaxReason" default="false" />
    <aura:attribute type="Boolean" name="showCountryForeignTax" default="false" />
    <aura:attribute type="Boolean" name="showLimitedAccountInfoModal" default="false" />
	<aura:attribute name="tempresidentdetailOptions" type="List" default="[{'label': 'Yes', 'value': 'Y'},{'label': 'No', 'value': 'N'}]" />
	<aura:attribute name="asylumDetailOptions" type="List" default="[{'label': 'Yes', 'value': 'Y'},{'label': 'No', 'value': 'N'}]" />
    <aura:attribute name="optionGiven" type="String" />
    <aura:attribute name="asylumoptionGiven" type="String" />
    <aura:attribute name="showTempResidentDetails" type="String"   />
    <aura:attribute name="showAsylumDetails" type="String"  />
    <aura:attribute type="Boolean" name="showFsp" default="false" /><!--Added By Mbuyiseni Mbhokane-->
    <aura:attribute type="Boolean" name="showCondition" default="false" /><!--Added By Mbuyiseni Mbhokane-->
    <aura:attribute type="Boolean" name="showNamedCondition" default="false" /><!--Added By Mbuyiseni Mbhokane-->
    <aura:Attribute name="isAgreedPrivacypolicy" type="boolean" default="false"/><!--Added By Haritha Police-->
    <aura:Attribute name="isAgreedVerificationPolicy" type="boolean" default="false"/><!-- Muvhuso: SPM onboarding -->
    <!--SPM attributes Added by Diksha 9/21/2020-->
	<aura:attribute name="isModalOpenforPM" type="boolean" default="false" />
	<!-- Diksha for SPM selection of PM-->
    <aura:attribute name="selectedPm" type="string" />
    <aura:attribute name="pmList" type="User[]" />
    <aura:attribute name="loggedinUserProfile" type="string" />
	<aura:attribute name="processNameP" type="String" />
	<!--6097 - Trigger Full Client On-boarding Prior Fulfilment -->
	<aura:attribute name="liteOpportunityId" type="String" />
	<!--6097 - Trigger Full Client On-boarding Prior Fulfilment -->
    <aura:attribute name="showRegulatoryQ1SubSection" type="boolean" default="false"/>
    <aura:attribute name="showRegulatoryQ2SubSection" type="boolean" default="false"/>
    <aura:attribute name="showRegulatoryQ1SubSection2" type="boolean" default="false"/>
    <aura:attribute name="showRegulatoryQ2SubSection2" type="boolean" default="false"/>
    <aura:attribute name="opportunityRecordId" type="String" /><!--Added By Masechaba Maseli to load Edit-->
    <aura:attribute name="showConsent" type="Boolean" default="true"/>
    <aura:attribute name="showSave" type="Boolean" default="true"/><!-- W-010629 to show/hide the buttton-->
    <aura:attribute name="showProvideDetails" type="boolean" default="false"/>
    <aura:attribute name="registerdChecked" type="Boolean" default="true"/>
    <aura:attribute name="showTemporaryResidentSection" type="boolean" default="false"/><!--W-013541-->
    <aura:attribute name="isTempResident" type="boolean" default="false"/><!--W-013541-->
    <aura:attribute name="isSAResident" type="boolean" default="false"/><!--W-013541-->
    <aura:if isTrue="{!v.showSpinner}">
        <div class="slds-spinner_container">
            <lightning:spinner aura:id="TheSpinner" variant="brand" size="medium"/>
        </div>
    </aura:if>
    <center style="z-index:99">
        <lightning:spinner aura:id="TheSpinner" class="slds-hide" variant="brand" size="large"/>
    </center>
    
    <!--Golden Source opoup preview-->
    <aura:if isTrue="{!v.isConfirmationModalOpen}">
		<section
			role="dialog"
			tabindex="-1"
			aria-labelledby="modal-heading-01"
			aria-modal="true"
			aria-describedby="modal-content-id-1"
			class="slds-modal slds-fade-in-open"
		>
            <div class="slds-modal__container">
                <header class="slds-modal__header">
                    <button class="slds-button slds-button_icon slds-modal__close slds-button_icon-inverse" title="Close">
                        <span class="slds-assistive-text">Close</span>
                    </button>
                    <h2 id="modal-heading-01" class="slds-modal__title slds-hyphenate">Golden Sources Preview</h2>
                </header>
                <div class="slds-modal__content slds-p-around_large" id="modal-content-id-1">
                    <div class="slds-grid slds-wrap slds-gutters">
						<div class="slds-col slds-size_1-of-2"><span>First Name</span><br /><span>{!v.hanisResponse.name}</span><br /><br /></div>
						<div class="slds-col slds-size_1-of-2"><span>ID Number</span><br /><span>{!v.hanisResponse.idNumber}</span><br /><br /></div>
						<div class="slds-col slds-size_1-of-2"><span>Surname</span><br /><span>{!v.hanisResponse.surname}</span></div>
                        <div class="slds-col slds-size_1-of-2">
                            <span>Address</span><br /><span>{!if(v.residentialAddressDetails != null, v.residentialAddressDetails.FullAddress, null)}</span>
                        </div>
                    </div>
                </div>
                <footer class="slds-modal__footer slds-modal__footer_directional">
					<lightning:button variant="neutral" label="Incorrect Info? Try searching again." title="Save" onclick="{!c.closeAccountPreviewModal}" />
					<lightning:button variant="brand" label="This info is correct. Proceed" title="Save" onclick="{!c.createIndividualAccount}" />
                </footer>
            </div>
        </section>
        <div class="slds-backdrop slds-backdrop_open"></div>
    </aura:if>
    
    <!-- TdB - Popup for no capture manually to cpature required minimal required details -->
    <aura:if isTrue="{!v.showLimitedAccountInfoModal}">
		<section
			role="dialog"
			tabindex="-1"
			aria-labelledby="modal-heading-01"
			aria-modal="true"
			aria-describedby="modal-content-id-1"
			class="slds-modal slds-fade-in-open"
		>
            <div class="slds-modal__container">
                <header class="slds-modal__header">
                    <button class="slds-button slds-button_icon slds-modal__close slds-button_icon-inverse" title="Close">
                        <span class="slds-assistive-text">Close</span>
                    </button>
                    <h2 id="modal-heading-01" class="slds-modal__title slds-hyphenate">Individual Onboarding</h2>
                </header>
                <div class="slds-modal__content slds-is-expanded slds-p-around_medium" id="modal-content-id-1">
                    <div class="slds-grid slds-wrap slds-gutters ">
						<lightning:recordEditForm
							aura:id="recordNewAccount"
                                                  recordId="{!v.accRecordId}"
                                                  recordTypeId="{!v.indvRecordTypeId}"
							objectApiName="Account"
						>
                            <lightning:layout multipleRows="true">
                                <lightning:layoutItem padding="" size="6" smallDeviceSize="12" mediumDeviceSize="6">
                                    <lightning:inputField fieldName="FirstName" aura:id="iFirstName" required="true" value="{!v.accountRecord.FirstName}"/>
                                </lightning:layoutItem>
                                <lightning:layoutItem padding="" size="6" smallDeviceSize="12" mediumDeviceSize="6">
									<lightning:inputField
										fieldName="Client_Group__c"
										aura:id="clientGroup"
										disabled="true"
										value="{!v.accountRecord.Client_Group__c}"
									/>
									<div aura:id="errclientGroup" class="slds-hide" style="color: var(--lwc-colorTextError, rgb(194, 57, 52)) !important">
										Please Enter Client Group as Non Individual
									</div>
                                </lightning:layoutItem>
                                <lightning:layoutItem padding="" size="6" smallDeviceSize="12" mediumDeviceSize="6">
                                    <lightning:inputField fieldName="LastName" aura:id="iLastName" required="true" value="{!v.accountRecord.LastName}"/>
                                </lightning:layoutItem>
                                <lightning:layoutItem padding="" size="6" smallDeviceSize="12" mediumDeviceSize="6">
									<lightning:inputField
										fieldName="Client_Type__c"
										aura:id="idClientType"
										disabled="true"
										value="{!v.accountRecord.Client_Type__c}"
									/>
									<div aura:id="errclientType" class="slds-hide" style="color: var(--lwc-colorTextError, rgb(194, 57, 52)) !important">
										Please Enter Client Type as Private Company
									</div>
                                </lightning:layoutItem>
                                <lightning:layoutItem padding="" size="6" smallDeviceSize="12" mediumDeviceSize="6" >
									<lightning:inputField
										fieldName="ID_Number__pc"
										aura:id="regNumber"
										label="ID Number"
										required="true"
										value="{!v.accountRecord.ID_Number__pc}"
									/>
                                </lightning:layoutItem>
                                <lightning:layoutItem padding="" size="6" smallDeviceSize="12" mediumDeviceSize="6">
                                    <lightning:inputField fieldName="ID_Type__c" aura:id="idIDType" required="true" value="{!v.accountRecord.ID_Type__c}" />
									<div aura:id="errclientType" class="slds-hide" style="color: var(--lwc-colorTextError, rgb(194, 57, 52)) !important">
										Please Enter ID Type
									</div>
                                </lightning:layoutItem>
                            </lightning:layout>
                        </lightning:recordEditForm>
                    </div>
                </div>
                <footer class="slds-modal__footer slds-modal__footer_directional">
					<lightning:button
						variant="neutral"
						class=""
						label="Cancel"
						title="Cancel"
						aura:id="cancelInfo"
						onclick="{!c.closeLimitedAccDetailsModal}"
					/>
					<lightning:button variant="brand" class="" label="Proceed to Onboarding" title="Save" aura:id="saveInfo" onclick="{!c.showBigForm}" />
                </footer>
            </div>
        </section>
        <div class="slds-backdrop slds-backdrop_open"></div>
    </aura:if>
    
    <!--<c:onboardingProgressIndicator currentStep="Complete Client Details" />-->
    <lightning:accordion activeSectionName="{! v.activeSections }" allowMultipleSectionsOpen="true">
        <!--CONSENT INFORMATION -->
        <aura:if isTrue="{!v.processType != 'SIC Code Change'}">
        <h1 class="slds-section__title">
            <span class="slds-align_absolute-center" title="Section Title">New-To-Bank Onboarding</span>
        </h1>
            <aura:set attribute="else">
                 <h1 class="slds-section__title">
            		<span class="slds-align_absolute-center" title="Section Title">{!v.ProcessName}</span>
        		</h1>
            </aura:set>
        </aura:if>
        <aura:if isTrue="{!v.showConsent}">
            <lightning:accordionSection name="Consent" label="New Client Consent Declaration for Information Retrieval">
                <div class="slds-form-element" aura:id="idNoError">
                    <div class="slds-form-element__control">
						<lightning:input
							disabled="{!v.disableConsentCheckbox}"
							name="IdNumber"
							aura:id="iIdNo"
							label="ID Number"
							placeholder="Type here..."
							value="{!v.IdentityNumber}"
						/>
                    </div>
                    <div class="slds-form-element__help slds-hide" id="form-error-01 ">This field is required</div>
                </div>
                
				<lightning:input
					disabled="{!v.disableConsentCheckbox}"
					type="checkbox"
					label="I AGREE TO ABSA BANK ACCESSING MY RECORDS ON NATIONAL IDENTIFICATION INFORMATION SOURCES AND ABSA BANK LIMITED TERMS &amp; CONDITIONS"
					name="Agree Golden Sources"
					onclick="{!c.showPopulateDetailsButton}"
					aura:id="agreed"
				/>
                <!--W-005816 - Haritha 10092020 - Consent for spm Onboarding -->   
                <aura:if isTrue="{!v.ProcessName == 'SPMOnboarding'}">
					<lightning:input
						type="checkbox"
						label="ABSA MAY SHARE INFORMATION REGARDING MY APPLICATION WITH THIRD PARTIES IN ACCORDANCE WITH THE PRIVACY POLICY"
						name="AgreePrivacyPolicy"
						onchange="{!c.savecheckedaccount}"
						aura:id="AgreePrivacyPolicy"
					/>
                    <!--W-007037 - Muvhuso 03112021 - Consent for spm Onboarding -->
					<lightning:input
						type="checkbox"
						label="I GIVE ABSA PERMISSION TO SOURCE AND USE MY DETAILS FOR IDENTIFICATION AND VERIFICATION PURPOSES IN ACCORDANCE WITH THE FINANCIAL INTELLIGENCE CENTRE ACT AND OTHER APPLICABLE LEGISLATION."
						name="AgreePrivacyPolicy"
						onchange="{!c.savecheckedAccountConsent}"
						aura:id="AgreeVerificatonPolicy"
					/>
                </aura:if>
                <aura:if isTrue="{!v.processType != 'SIC Code Change'}">
                <div class="c-container">
                    <lightning:layout multipleRows="true">
                        <lightning:layoutItem padding="around-small" size="12" class="slds-align_absolute-center">
                            <!--<div class="slds-hide" aura:id="populateDiv">-->
							<lightning:button
								disabled="{!v.goldenSourceConsentGiven}"
								variant="brand"
								class="slds-m-top_xx-large"
								label="Populate Details from Golden Sources"
								title="Populate Details from Golden Sources"
								onclick="{! c.populateDetails }"
							/>
							<lightning:button
								disabled="{!not(v.goldenSourceConsentGiven)}"
								variant="brand"
								class="slds-m-top_xx-large"
								label="Capture Information Manually"
								title="No Consent Given"
								onclick="{! c.openLimitedAccDetailsModal }"
							/>
                            <!-- </div> -->
                        </lightning:layoutItem>
                    </lightning:layout>
                </div> 
                </aura:if>
            </lightning:accordionSection>
        </aura:if>
        
        <aura:if isTrue="{!v.showRecordEditForm}">
			<div class="slds-hide" aura:id="goldenSourceDocs" style="margin-bottom: 40px">
                <br />
                
				<lightning:recordEditForm
					aura:id="recordViewForm22"
                                          recordId="{!v.accRecordId}"
                                          objectApiName="Account"
                                          onsubmit="{!c.handleOnSubmit}"
                                          onsuccess="{!c.handleOnSuccess}"
                                          onerror="{!c.handleOnError}"
					recordTypeId="{!v.indvRecordTypeId}"
				>
                    <!-- save W-010629-->
                    <aura:if isTrue="{!and(v.showSave, v.accRecordId!= null)}">
                        <div class="slds-align_absolute-center">
                            <lightning:button type="submit" variant="brand" label="Save and Continue" onclick="{!c.onSave}"/>
                        </div>
                    </aura:if>
                    <!--ACCOUNT INFORMATION START-->
                    <lightning:accordionSection name="Account" label="Account Information">
                        <div class="slds-grid slds-wrap slds-gutters">
                            <lightning:layout multipleRows="true">
                                <aura:if isTrue="{!v.clientTypeValue == 'Deceased Estate'}">
                                    <lightning:layoutItem padding="" size="6" smallDeviceSize="12" mediumDeviceSize="6">
										<lightning:input
											disabled="{!v.isFormReadOnly}"
											aura:id="entityType"
											label="Entity Name"
											required="true"
											value="{!v.clientGroupValue +'-'+ v.clientTypeValue+ '-'+ v.placeofResValue}"
										/>
                                    </lightning:layoutItem>
                                </aura:if>
                                
                                <!--TdB - Show/Hide fields based on New to Bank/Lite Onboarding -->
                                <aura:if isTrue="{!v.processType != 'Lite Onboarding'}">
                                    <lightning:layoutItem padding="" size="6" smallDeviceSize="12" mediumDeviceSize="6">
                                        <lightning:inputField fieldName="PersonTitle" aura:id="iPersonTitle" required="true"/>
                                    </lightning:layoutItem>
                                    <lightning:layoutItem padding="" size="6" smallDeviceSize="12" mediumDeviceSize="6">
                                        <lightning:inputField fieldName="Initials__pc" aura:id="iInitials" required="true"/>
                                    </lightning:layoutItem>
                                </aura:if>
                                
                                <lightning:layoutItem padding="" size="6" smallDeviceSize="12" mediumDeviceSize="6">
                                    <lightning:inputField fieldName="FirstName" aura:id="iFirstName" label="Account Name" required="true"/>
									<div aura:id="errFirstName" class="slds-hide" style="color: var(--lwc-colorTextError, rgb(194, 57, 52)) !important">
										Please Enter First Name
									</div>
                                </lightning:layoutItem>
                                <lightning:layoutItem padding="" size="6" smallDeviceSize="12" mediumDeviceSize="6">
                                    <lightning:inputField fieldName="LastName" aura:id="iLastName" label="Account Name" required="true"/>
									<div aura:id="errLastNam" class="slds-hide" style="color: var(--lwc-colorTextError, rgb(194, 57, 52)) !important">
										Please Enter LastName
									</div>
                                </lightning:layoutItem>
                                <lightning:layoutItem padding="" size="6" smallDeviceSize="12" mediumDeviceSize="6">
                                    <lightning:inputField fieldName="ID_Type__c" aura:id="idType" required="true"/>
									<div aura:id="erridType" class="slds-hide" style="color: var(--lwc-colorTextError, rgb(194, 57, 52)) !important">
										Please Enter ID Type
									</div>
                                </lightning:layoutItem>
                                
                                 <!--TdB - Show/Hide fields based on New to Bank/Lite Onboarding -->
                            	<aura:if isTrue="{!v.processType != 'SIC Code Change'}">
                                <aura:if isTrue="{!v.processType != 'Lite Onboarding'}">
                                    <lightning:layoutItem padding="" size="6" smallDeviceSize="12" mediumDeviceSize="6">
                                        <lightning:inputField fieldName="Primary_Identification_Reason__c" aura:id="identReason"/>
                                    </lightning:layoutItem>
                                    
                                    <aura:if isTrue="{!v.processType != 'Surety Onboarding'}">
                                        <aura:if isTrue="{!!and(and(v.isMinorIndiv == true), v.isForeignNational == false)}">
                                            <lightning:layoutItem padding="" size="6" smallDeviceSize="12" mediumDeviceSize="6">
                                                <lightning:inputField fieldName="Client_Agreement__c" aura:id="iClientAgreement" required="true"/>
                                            </lightning:layoutItem>
                                        </aura:if>
                                    </aura:if>
                                </aura:if>
                                </aura:if>
                                
                                <aura:if isTrue="{!v.isForeignNational == false}">
                                <lightning:layoutItem padding="" size="6" smallDeviceSize="12" mediumDeviceSize="6" >
                                    <lightning:inputField fieldName="ID_Number__pc" aura:id="iIDNumber" label="ID Number" required="true"/>
										<div aura:id="errIDNumber" class="slds-hide" style="color: var(--lwc-colorTextError, rgb(194, 57, 52)) !important">
											Please Enter ID Type
										</div>
                                </lightning:layoutItem>
                                </aura:if>
                                
                                <!--TdB - Show/Hide fields based on New to Bank/Lite Onboarding -->
                            	<aura:if isTrue="{!v.processType != 'SIC Code Change'}">
                                <aura:if isTrue="{!v.processType != 'Lite Onboarding'}">
                                    <lightning:layoutItem padding="" size="6" smallDeviceSize="12" mediumDeviceSize="6" >
                                        <lightning:inputField fieldName="Date_Issued__pc" aura:id="iDateIssued"/>
                                    </lightning:layoutItem>
                                    <lightning:layoutItem padding="" size="6" smallDeviceSize="12" mediumDeviceSize="6">
                                        <lightning:inputField fieldName="Gender__pc" aura:id="iGender" required="true"/>
                                    </lightning:layoutItem>
                                </aura:if>
                                </aura:if>
                                
                                <lightning:layoutItem padding="" size="6" smallDeviceSize="12" mediumDeviceSize="6">
									<lightning:inputField
										fieldName="Client_Type__c"
										aura:id="iClientType"
										onchange="{!c.setSectionsToRender}"
										disabled="true"
									/>
									<div aura:id="errclientType" class="slds-hide" style="color: var(--lwc-colorTextError, rgb(194, 57, 52)) !important">
										Please Enter Client Type
									</div>
                                </lightning:layoutItem>
                                <lightning:layoutItem padding="" size="6" smallDeviceSize="12" mediumDeviceSize="6">
									<lightning:inputField
										fieldName="Client_Group__c"
										aura:id="iClientGroup"
										onchange="{!c.setSectionsToRender}"
										disabled="true"
									/>
									<div aura:id="errclientGroup" class="slds-hide" style="color: var(--lwc-colorTextError, rgb(194, 57, 52)) !important">
										Please Enter Client Group as Non Individual
									</div>
                                </lightning:layoutItem>
                                
                                <!--TdB - Show/Hide fields based on New to Bank/Lite Onboarding -->
                            	<aura:if isTrue="{!v.processType != 'Lite Onboarding'}">
                                    <lightning:layoutItem padding="" size="6" smallDeviceSize="12" mediumDeviceSize="6">
                                        <lightning:inputField fieldName="Business_Unit__c" aura:id="iBusinessUnit" required="true"/>
										<div aura:id="errbusinessUnit" class="slds-hide" style="color: var(--lwc-colorTextError, rgb(194, 57, 52)) !important">
											Please Enter Business Unit
										</div>
                                    </lightning:layoutItem>
                                </aura:if>
                                
                                <lightning:layoutItem padding="" size="6" smallDeviceSize="12" mediumDeviceSize="6">
