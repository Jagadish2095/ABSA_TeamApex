                                        <aura:iteration items="{! v.employerSectorOptions }" var="e">
                                            <option value="{!e}" selected="{!e==v.employerSector}">{!e}</option>
                                        </aura:iteration>
                                    </lightning:select>
                                </div>
                            </lightning:layoutitem>
                        </lightning:layout>

                        <lightning:layout>
