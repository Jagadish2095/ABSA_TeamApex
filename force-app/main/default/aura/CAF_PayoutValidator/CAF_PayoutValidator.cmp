    <aura:handler name="init" value="{!this}" action="{!c.doInit}" />
    <aura:handler event="force:refreshView" action="{!c.doInit}" />
    
    <lightning:notificationsLibrary aura:id="notifLib" />
    <!--<div class="mainDiv" style="{!or(v.sendToVal =='Payout Validator',v.sendToVal == '') ? '' : 'pointer-events:none!important' }">-->	
    <div class="mainDiv" style="{!!v.isCompReadOnly ? '' : 'pointer-events:none!important' }">
        <lightning:recordEditForm aura:id="payoutvalidator" objectApiName="Case" recordId="{!v.recordId}" onerror="{!c.handleError}">
            <lightning:accordion activeSectionName="{! v.activeSections }" aura:id="accordianId" allowMultipleSectionsOpen="true">
                <lightning:accordionSection name="checkList"  label="Payout Validator Checklist">
                    <div>
                        <p><b>Payout Validation - CAF</b></p>
                    </div>
                    
                    <div class="slds-grid slds-wrap slds-gutters_medium">
                        <div class="slds-col slds-size_1-of-2">
                            <span>
                                <lightning:radioGroup aura:id="LandlordAddressGroup"
                                                      label="Names of address of landlord received"
                                                      options="{!v.select}" 
                                                      value="{!v.landlordAddressVal}"
                                                      type="button"
                                                      onchange="{!c.landlordClick}"/>
                                
                                <lightning:inputField aura:id="landlordAddressValId" fieldName="pv_Names_of_address_of_landlord_received__c" class="slds-hide"/> 
                                
                            </span>
                        </div>               
                        <div class="slds-col slds-size_1-of-2">
                            <span>
                                <lightning:radioGroup aura:id="Form25CompletedGroup"
                                                      label="Form 25 completed in full and signed"
                                                      options="{!v.select}" 
                                                      value="{!v.form25CompleteVal}"
                                                      type="button"
                                                      onchange="{!c.form25Click}"/>
                                
                                <lightning:inputField aura:id="form25CompleteValId" fieldName="pv_Form_25_completed_in_full_and_signed__c" class="slds-hide"/>
                            </span>
                        </div>
                    </div>
                    <br/>
                    <div>
                        <p><b>Payout Validation - Signed Contract</b></p>
                    </div>
                    <div class="slds-grid slds-wrap slds-gutters_medium">
                        <div class="slds-col slds-size_1-of-2">
                            <span>
