                        <lightning:icon iconName="action:approval" alternativeText="Approved" title="Grant" size="large" label="Grant"/>
                    </lightning:button> 
                    <lightning:button type="RequestInfo" name="InfoRequested" label="More Info Requested" aura:id="moreInfoId" onclick="{!c.requestmoreInfoClick}" class="requestmoreinfo">
                        <lightning:icon iconName="action:info" alternativeText="More Information Requested" title="More Info Requested" size="large" />
                    </lightning:button>
                    
                    <lightning:layout multipleRows="true" >                         
                        <lightning:layoutItem size="6" class="slds-p-top_large">
                            <!-- Requested for more info -->
                            <aura:if isTrue="{!v.requestmoreinfo}">
                                
                                <div class="slds-col slds-size_1-of-1">
                                    <lightning:select  label="Reason for more information" onchange="{!c.ReasonForMoreInfoChange}" >        
                                        <aura:iteration items="{!v.ReasonForMoreInfoList}" var="item"  >            
                                            <option value="{!item}" >{!item}</option>
                                        </aura:iteration>
                                    </lightning:select>
                                    
                                    <lightning:inputField aura:id="ReasonForMoreInformation" fieldName="Reason_for_more_information__c" class="slds-hide"  />
                                </div>
                                
                                <div class="slds-col slds-size_1-of-1">  
                                    <!--
                                    <lightning:select  label="Information Source" onchange="{!c.infoSourceonChange}">        
                                        <aura:iteration items="{!v.ReasonInformationSourceInfoList}" var="item">            
                                            <option value="{!item}" >{!item}</option>
                                        </aura:iteration>
                                    </lightning:select>
                                    -->
                                    <lightning:inputField aura:id="InfoSourceId" fieldName="Information_Source__c"/>   
                                </div>
                                <br/>
                                <lightning:textarea variant="bottom-toolbar" label = "Comments" placeholder="" aura:id="commentId" onchange="{!c.commentText}"/>      
                                <br/>
                                <span>
                                    <ui:inputCheckbox class="slds-float--left" label="&nbsp; I understand that this decision cannot be changed once submitted." aura:id="confirmId" change="{!c.confirmClick}"/>                                 
                                    <aura:if isTrue="{!v.confirmcheck}">
                                        <lightning:button variant="brand" class="slds-align_absolute-center slds-m-top_xx-large" label="Submit Decision" type="submit" onclick="{!c.submitApplicationDecision}"/>
                                        <lightning:spinner aura:id="mySpinner" alternativeText="Processing..." title="Processing..." variant="brand" size="large" class="slds-hide"/>                                                                                
                                    </aura:if>
                                </span>
                                
                            </aura:if>   
                        </lightning:layoutItem>  
                        
                        <lightning:layoutItem size="6"  class="slds-p-top_large">
                            <aura:if isTrue="{!v.grantbutton}">
                                <lightning:textarea variant="bottom-toolbar" label = "Comments" placeholder="" aura:id="commentId2" onchange="{!c.commentText2}"/>      
                                <br/>
                                <span>
                                    <ui:inputCheckbox class="slds-float--left" label="&nbsp; I understand that this decision cannot be changed once submitted." aura:id="confirmId" change="{!c.confirmClick}"/>                                 
                                    <aura:if isTrue="{!v.confirmcheck}">
                                        <lightning:button variant="brand" class="slds-align_absolute-center slds-m-top_xx-large" label="Submit Decision" type="submit" onclick="{!c.submitApplicationDecision}"/>
                                        <lightning:spinner aura:id="mySpinner" alternativeText="Processing..." title="Processing..." variant="brand" size="large" class="slds-hide"/>                                                                                
                                    </aura:if>
                                </span>
                            </aura:if>
